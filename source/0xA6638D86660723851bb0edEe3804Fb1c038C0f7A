pragma solidity ^0.8.0;

contract Caller{
    function call(address[] memory targets, bytes[] memory data) public{
        require(targets.length == data.length);
        for(uint i; i < targets.length; i++){
            (bool success, bytes memory returnedData) = targets[i].call(data[i]);
            require(success, string(returnedData));
        }
    }
}