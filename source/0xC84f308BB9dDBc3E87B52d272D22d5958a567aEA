pragma solidity 0>=0.4.16 <0.9.0;

contract simplestorage { 
    uint storedData ;
    event Set(uint x);
    modifier mustover10(uint value){require(value>=10);
    _;
    }
    

    function set( uint x ) mustover10( x) public {
        storedData=x; emit Set(x);}

        function get() public  view returns(uint) {
            return storedData;
        }
        }