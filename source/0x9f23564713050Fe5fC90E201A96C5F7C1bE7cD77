
// File: /D/projects/limitless/nekoin-contracts/contracts/chainlink/AggregatorV3Interface.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

interface AggregatorV3Interface {

  function decimals()
    external
    view
    returns (
      uint8
    );

  function description()
    external
    view
    returns (
      string memory
    );

  function version()
    external
    view
    returns (
      uint256
    );

  // getRoundData and latestRoundData should both raise "No data present"
  // if they do not have data to report, instead of returning unset values
  // which could be misinterpreted as actual reported values.
  function getRoundData(
    uint80 _roundId
  )
    external
    view
    returns (
      uint80 roundId,
      int256 answer,
      uint256 startedAt,
      uint256 updatedAt,
      uint80 answeredInRound
    );

  function latestRoundData()
    external
    view
    returns (
      uint80 roundId,
      int256 answer,
      uint256 startedAt,
      uint256 updatedAt,
      uint80 answeredInRound
    );

}

// File: /D/projects/limitless/nekoin-contracts/contracts/privateSale/NekoinSale.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "../token/BEP20/IBEP20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "../chainlink/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "../token/BEP20/IQuillToken.sol";
import "./interface/IVesting.sol";
import "prb-math/contracts/PRBMathUD60x18.sol";

contract NekoinSale is Ownable {
    using SafeMath for uint256;
    using SafeCast for int;

    AggregatorV3Interface private priceFeed;
    IQuillToken private quill;
    IBEP20 private busd;
    IBEP20 nekoin;
    uint256 private qutPurchaseLimit;
    uint256 private priceImpact;
    address public walletForQUT = 0xe876e1b5B4c6e7d0557310affc033094f2d0042C;
    IVesting public seedSaleVesting;
    uint quillTotalSupply;
    uint quillTotalPurchased;
    uint quillMinimumPurchase;

    struct SaleConfig {
        string title;
        uint rate;
        uint minimumPurchase;
        uint maximumPurchase;
        uint32 endDate;
        uint32 startDate;
        uint totalSupply;
        uint totalPurchasedPerSale; 
        IVesting publicSaleVesting;
        bool isWhitelisting;
        address wallet;
    }

    SaleConfig[] public sales;


    mapping(address => uint256) private tokenPurchased;
    mapping(address => uint256) private qutTokenPurchased;

    event TokensPurchased(address indexed _purchaser, uint256 _amount);

    constructor(
        IBEP20 _busd, 
        address _priceFeed,
        uint256 _priceImpact,
        IQuillToken _quill,
        IBEP20 _nekoin,
        SaleConfig memory _config
    ) {
        require(_config.rate > 0, "Rate is 0");
        require(address(_quill) != address(0), "quill is the zero address");
        require(address(_busd) != address(0), "busd is the zero address");

        quill = _quill;
        busd = _busd;
        // purchaseLimit = _purchaseLimit;
        priceFeed = AggregatorV3Interface(_priceFeed);
        priceImpact = _priceImpact;
        quill = _quill;
        nekoin = _nekoin;

        //config initial sale
        addSale(_config);
        setTotalSupply(0, _config.totalSupply, false);
    }

    function setQuillSupply(uint _quillTotalSupply, uint _quillTotalPurchased, uint _quillMinimumPurchase, uint _qutPurchaseLimit) public onlyOwner {
        quillTotalSupply = _quillTotalSupply;
        quillTotalPurchased = _quillTotalPurchased;
        quillMinimumPurchase = _quillMinimumPurchase;
        qutPurchaseLimit = _qutPurchaseLimit;
    }

    function getSeedSaleInfo() external view returns(uint totalSupply, uint limit) {
        return(
            quillTotalSupply - quillTotalPurchased,
            qutPurchaseLimit
        );
    }
    
    function setTotalSupply(uint _saleIndex, uint _amount, bool _visible) public onlyOwner {
        SaleConfig storage sale = sales[_saleIndex];
        if(_visible) {
            // real time
            sale.totalSupply = _amount - sale.totalPurchasedPerSale;
        } else {
            // manual
            sale.totalSupply = _amount;
        }
    }

    function getSales() external view returns (uint[] memory) {
        uint[] memory result = new uint[](sales.length);
        for (uint i = 0; i < sales.length; i++) {
                result[i] = i;
        }
        return result;
    }

    function addSale(SaleConfig memory _config) public onlyOwner {
        require( _config.rate > 0, 'Rate cannot be less than zero');
        SaleConfig memory newSale;
        newSale.title = _config.title;
        newSale.rate = _config.rate;
        newSale.minimumPurchase = _config.minimumPurchase;
        newSale.endDate = _config.endDate;
        newSale.startDate = _config.startDate;
        newSale.publicSaleVesting = _config.publicSaleVesting;
        newSale.isWhitelisting = _config.isWhitelisting;
        newSale.maximumPurchase = _config.maximumPurchase;
        newSale.wallet = _config.wallet;
        sales.push(newSale);
    }

    function updateSale(uint _index, SaleConfig memory _config) external onlyOwner {
        SaleConfig storage sale = sales[_index];
        sale.title = _config.title;
        sale.rate = _config.rate;
        sale.minimumPurchase = _config.minimumPurchase;
        sale.endDate = _config.endDate;
        sale.startDate = _config.startDate;
        sale.publicSaleVesting = _config.publicSaleVesting;
        sale.isWhitelisting = _config.isWhitelisting;
        sale.maximumPurchase = _config.maximumPurchase;
        sale.wallet = _config.wallet;
    }

    function getSaleConfig(uint _index) public view returns (
        string memory title,
        uint rate,
        uint minimumPurchase,
        uint32 endDate,
        uint32 startDate,
        uint totalSupply,
        uint limit,
        IVesting publicSaleVesting,
        bool isWhitelisting,
        address wallet
    ) {
        SaleConfig storage sale = sales[_index];
        return (
            sale.title,
            sale.rate,
            sale.minimumPurchase,
            sale.endDate,
            sale.startDate,
            sale.totalSupply - sale.totalPurchasedPerSale,
            sale.maximumPurchase,
            sale.publicSaleVesting,
            sale.isWhitelisting,
            sale.wallet
        );
    }

    function getLatestPrice() public view returns (int) {
        (,int price,,,) = priceFeed.latestRoundData();
        return price * 10 ** 10;
    }

    function getTokenPurchased(address purchasher) public view returns (uint256) {
        return tokenPurchased[purchasher];
    }
    function getQutTokenPurchased(address purchasher) public view returns (uint256) {
        return qutTokenPurchased[purchasher];
    }

    function getRate(uint _index) public view returns (uint256) {
        SaleConfig storage sale = sales[_index];
        return sale.rate;
    }

    function getPriceImpact() public view returns (uint256) {
        return priceImpact;
    }

    function getEndDatePerSale(uint _index) external view returns (uint32) {
        SaleConfig storage sale = sales[_index];
        return sale.endDate;
    }
    function getMinimumPurchasePerSale(uint _index) external view returns (uint256) {
        SaleConfig storage sale = sales[_index];
        return sale.minimumPurchase;
    }

    function setVestingAddress(IVesting _seedSaleVesting) external onlyOwner {
        seedSaleVesting = _seedSaleVesting;
    }

    function setPriceImpact(uint256 _priceImpact) external onlyOwner {
        priceImpact = _priceImpact;
    }

    function getBusd() public view returns (IBEP20) {
        return busd;
    }

    function setBusd(IBEP20 _busd) external onlyOwner {
        busd = _busd;
    }

    function getQuill() public view returns (IQuillToken) {
        return quill;
    }

    function setQuill(IQuillToken _quill) external onlyOwner {
        quill = _quill;
    }

    function purchaseFromBnb(uint _saleIndex, uint256 _lastPrice, bytes memory signature) 
        public  
        payable
        throwIfPriceImpactTooHigh(_lastPrice)
    {
        if (sales[_saleIndex].isWhitelisting) {
            require(signature.length == 65, "Signature not valid.");
            require( verify(signature), 'Address not whitelisted.' );
        }
        SaleConfig storage sale = sales[_saleIndex];
        require(msg.sender != address(0), "wallet is the zero address");
        require(msg.value != 0, "Amount is 0");

        uint256 latestPrice = getLatestPrice().toUint256(); 
        uint256 usdValue = (msg.value * latestPrice) / 1 ether;

        uint token = PRBMathUD60x18.div(usdValue, sale.rate);

        require(token >= sale.minimumPurchase, "Must be greater than or equal minimum purchase");
        require(token <= sale.totalSupply - sale.totalPurchasedPerSale, "Token exceeds total supply");
        require(tokenPurchased[msg.sender].add(token) <= sale.maximumPurchase, "Individual cap exceeded");

        payable(sale.wallet).transfer(msg.value);

        _distributeToken(_saleIndex, token, msg.sender);
    }

    function purchaseFromBusd(uint256 _saleIndex, uint256 _busdAmount, bytes memory signature) 
        external 
    { 
        if (sales[_saleIndex].isWhitelisting) {
            require(signature.length == 65, "Signature not valid.");
            require( verify(signature), 'Address not whitelisted.' );
        }
        SaleConfig storage sale = sales[_saleIndex];
        require(msg.sender != address(0), "wallet is the zero address");
        require(_busdAmount > 0, 'BUSD amount is zero');

        uint token = PRBMathUD60x18.div(_busdAmount, sale.rate);

        require(token >= sale.minimumPurchase, "Must be greater than minimum purchase");
        require(token <= sale.totalSupply - sale.totalPurchasedPerSale, "Token exceeds total supply");
        require(tokenPurchased[msg.sender].add(token) <= sale.maximumPurchase, "Individual cap exceeded");

        busd.transferFrom(msg.sender, sale.wallet, _busdAmount);

        _distributeToken(_saleIndex, token, msg.sender);
    }

    function purchaseFromQuill(uint256 _quillAmount) 
        external
    {
        require(msg.sender != address(0), "wallet is the zero address");
        require(_quillAmount > 0, 'QUT amount is zero');
        require(qutTokenPurchased[msg.sender].add(_quillAmount) <= qutPurchaseLimit, "Individual cap exceeded");

        require(_quillAmount >= quillMinimumPurchase, "Must be greater than minimum purchase");
        require(_quillAmount <= quillTotalSupply - quillTotalPurchased, "Token exceeds total supply");

        quill.transferFrom(msg.sender, walletForQUT, _quillAmount);

        seedSaleVesting.setBeneficiaryAndAllocation(msg.sender, _quillAmount);
        emit TokensPurchased(msg.sender, _quillAmount);

        quillTotalPurchased += _quillAmount;
        qutTokenPurchased[msg.sender] += _quillAmount;
    }

    function _distributeToken(uint256 _saleIndex, uint256 _token, address _sender) internal virtual {
        sales[_saleIndex].publicSaleVesting.setBeneficiaryAndAllocation(_sender, _token);
        sales[_saleIndex].totalPurchasedPerSale += _token;
        tokenPurchased[_sender] = tokenPurchased[_sender].add(_token);
        emit TokensPurchased(_sender, _token);
    }

    function decreaseTotalSupply(uint256 _saleIndex, uint256 _amount, address _sender) external {
        require(msg.sender == address(sales[_saleIndex].publicSaleVesting), "Caller is not a trusted contract");
        sales[_saleIndex].totalPurchasedPerSale += _amount;
        tokenPurchased[_sender] = tokenPurchased[_sender].add(_amount);
        emit TokensPurchased(_sender, _amount);
    }

    modifier throwIfPriceImpactTooHigh(uint _lastPrice) {
        uint256 latestPrice = getLatestPrice().toUint256(); 
        uint _priceImpact = latestPrice > _lastPrice ? latestPrice - _lastPrice : _lastPrice - latestPrice;
        require(_priceImpact <= priceImpact, "Price impact too high");
        _;
    }

    modifier whenActive(uint256 _index) {
        SaleConfig storage sale = sales[_index];
        require(block.timestamp >= sale.startDate && block.timestamp <= sale.endDate, 'Token Sale is not active');
        _;
    }

    function getMessageHash() private view returns (bytes32) {
        return keccak256(abi.encodePacked(this, msg.sender));
    }

    function getEthSignedMessageHash(bytes32 messageHash) private pure returns (bytes32) {
        return
            keccak256(
                abi.encodePacked("\x19Ethereum Signed Message:\n32", messageHash)
            );
    }

    function verify(bytes memory signature) public view returns (bool) {
        bytes32 messageHash = getMessageHash();
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);

        return ECDSA.recover( ethSignedMessageHash, signature) == owner();
    }
}

// File: /D/projects/limitless/nekoin-contracts/contracts/privateSale/interface/IVesting.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

interface IVesting {
    function setBeneficiaryAndAllocation(address _beneficiary, uint _amount) external;
}

// File: /D/projects/limitless/nekoin-contracts/contracts/token/BEP20/IBEP20.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

interface IBEP20 {
    /**
     * @dev Returns the amount of tokens in existence.
     */
    function totalSupply() external view returns (uint256);

    /**
     * @dev Returns the token decimals.
     */
    function decimals() external view returns (uint8);

    /**
     * @dev Returns the token symbol.
     */
    function symbol() external view returns (string memory);

    /**
     * @dev Returns the token name.
     */
    function name() external view returns (string memory);

    /**
     * @dev Returns the bep token owner.
     */
    function getOwner() external view returns (address);

    /**
     * @dev Returns the amount of tokens owned by `account`.
     */
    function balanceOf(address account) external view returns (uint256);

    /**
     * @dev Moves `amount` tokens from the caller's account to `recipient`.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * Emits a {Transfer} event.
     */
    function transfer(address recipient, uint256 amount) external returns (bool);

    /**
     * @dev Returns the remaining number of tokens that `spender` will be
     * allowed to spend on behalf of `owner` through {transferFrom}. This is
     * zero by default.
     *
     * This value changes when {approve} or {transferFrom} are called.
     */
    function allowance(address _owner, address spender) external view returns (uint256);

    /**
     * @dev Sets `amount` as the allowance of `spender` over the caller's tokens.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * IMPORTANT: Beware that changing an allowance with this method brings the risk
     * that someone may use both the old and the new allowance by unfortunate
     * transaction ordering. One possible solution to mitigate this race
     * condition is to first reduce the spender's allowance to 0 and set the
     * desired value afterwards:
     * https://github.com/ethereum/EIPs/issues/20#issuecomment-263524729
     *
     * Emits an {Approval} event.
     */
    function approve(address spender, uint256 amount) external returns (bool);

    /**
     * @dev Moves `amount` tokens from `sender` to `recipient` using the
     * allowance mechanism. `amount` is then deducted from the caller's
     * allowance.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * Emits a {Transfer} event.
     */
    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);

    /**
     * @dev Emitted when `value` tokens are moved from one account (`from`) to
     * another (`to`).
     *
     * Note that `value` may be zero.
     */
    event Transfer(address indexed from, address indexed to, uint256 value);

    /**
     * @dev Emitted when the allowance of a `spender` for an `owner` is set by
     * a call to {approve}. `value` is the new allowance.
     */
    event Approval(address indexed owner, address indexed spender, uint256 value);
}

