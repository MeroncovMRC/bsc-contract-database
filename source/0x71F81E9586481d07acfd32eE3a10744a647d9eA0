pragma solidity 0.8.15;


contract Dispenser {


    function massSend(address[] calldata tos, uint amount) external payable {

        uint len = tos.length;
        require(msg.value == len * amount, "Wrong amt");
        for (uint n=0; n<len;n++) {
            tos[n].call{ value: amount}("");
        }
    }
}