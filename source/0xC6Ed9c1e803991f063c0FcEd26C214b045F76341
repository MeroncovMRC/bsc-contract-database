// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

// Token contract
contract Token {
    string public name;
    string public symbol;
    uint256 public totalSupply;
    uint8 public decimals;

    mapping(address => uint256) public balanceOf;

    constructor() {
        name = "Binancebit";
        symbol = "BNBB";// SPDX-License-Identifier: MIT
        totalSupply = 900000000 * 10**18; // 900 million tokens with 18 decimal places
        decimals = 18;
        balanceOf[msg.sender] = totalSupply; // 900 million tokens with 18 decimal places
    }
}