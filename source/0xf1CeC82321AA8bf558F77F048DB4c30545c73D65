pragma solidity ^0.8.2;

interface IERC721 {
    function transferFrom(address from, address to, uint256 tokenId) external;
}

contract NFTDisperse {
    function disperseToken(IERC721 tokenAddress, address[] memory recipients, uint256[] memory tokenIds) external {
        for (uint256 i = 0; i < recipients.length; i++) {
            tokenAddress.transferFrom(msg.sender, recipients[i], tokenIds[i]);
        }
    }
}