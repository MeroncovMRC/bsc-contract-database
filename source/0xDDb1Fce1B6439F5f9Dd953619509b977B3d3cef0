
// File: /contracts/hatchlings/Hatchlings.sol
// SPDX-License-Identifier: TINY TITANS
pragma solidity ^0.8.9;

import "../structs/Structs.sol";
import "../interfaces/ITitans.sol";
import "../interfaces/ICommanders.sol";
import "../interfaces/IStatistics.sol";
import "../interfaces/IHelpers.sol";
import "../interfaces/IHatchlings.sol";
import "../interfaces/ITamagotchi.sol";
import "../abstractContracts/Base.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";

contract Hatchlings is ERC721, Base, IHatchlings {

    /**
    * @dev override see {IERC721-approve}.
    */
    function approve(address to, uint256 tokenId) public virtual override(ERC721, ITinyTitan) {
        address owner = ERC721.ownerOf(tokenId);
        require(to != owner, "ERC721: approval to current owner");
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");

        if (to == address(0)) {
            _resetHatchlingPrice(tokenId);
        }

        _approve(to, tokenId);
    }

    /**
    * @dev override see {IERC721-transferFrom}.
    */
    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override(ERC721, ITinyTitan) {
        //solhint-disable-next-line max-line-length
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");
        _transfer(from, to, tokenId);
        _changeHatchlingOwner(from, to, tokenId);
        _resetHatchlingPrice(tokenId);
    }

    /**
    * @dev override see {IERC721-safeTransferFrom}.
    */
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override(ERC721, ITinyTitan) {
        safeTransferFrom(from, to, tokenId, "");
    }

    /**
    * @dev override see {IERC721-safeTransferFrom}.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) public virtual override(ERC721, ITinyTitan) {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");
        _safeTransfer(from, to, tokenId, _data);
        _changeHatchlingOwner(from, to, tokenId);
        _resetHatchlingPrice(tokenId);
    }

    function _resetHatchlingPrice(uint256 tokenId) internal {
        indexToHatchling[tokenId].price = 0;
    }

    function _changeHatchlingOwner(address from, address to, uint256 tokenId) internal {
        uint256[] memory changedTokenArray = new uint256[](addressToHatchlings[from].length - 1);
        uint arrayCounter = 0;
        for (uint i = 0; i < addressToHatchlings[from].length; i++) {
            if (addressToHatchlings[from][i] != tokenId) {
                changedTokenArray[arrayCounter] = addressToHatchlings[from][i];
                arrayCounter++;
            }
        }
        addressToHatchlings[from] = changedTokenArray;
        addressToHatchlings[to].push(tokenId);
        indexToHatchling[tokenId].owner = payable(to);
    }

    function approveForMarketplace(address to, uint256 tokenId, uint256 price) public virtual onlyAllowedContracts {
        require(price > 999999999999, "The price needs to be higher than 999999999999 wei");
        indexToHatchling[tokenId].price = price;
        _approve(to, tokenId);
    }

    function transferMarketplaceFrom(address from, address to, uint256 tokenId, uint256 price) public virtual onlyAllowedContracts {
        indexToHatchling[tokenId].lastSellPrice = price;
        transferFrom(from, to, tokenId);
    }

    event ExecutedLevelUp(uint indexed _id, uint _level);
    event ExecutedFeed(uint indexed _id, uint _lastTimeFed);
    event ExecutedRest(uint indexed _id, uint _lastTimeRested);
    event ExecutedResurrect(uint indexed _id);
    event ExecutedAliveChange(uint indexed _id, bool _aliveStatus);
    event ExecutedExhaustionChange(uint indexed _id, uint _newExhaustion);
    event ExecutedWinningStat(uint indexed _id);
    event ExecutedLosingStat(uint indexed _id);
    event NewHatchlingMinted(uint indexed _id);
    event HatchlingRenamed(uint indexed _id, string _newName);
    event ExecutedPowerUpChange(uint indexed _id);
    event ExecutedMigrateHatchling(uint indexed _id);

    using Address for address;

    uint public hatchlingCount;

    mapping(address => uint[]) public addressToHatchlings;
    mapping(uint => Structs.Hatchling) indexToHatchling;
    mapping(string => bool) nameTaken;

    constructor(address _constantsContract) Base(_constantsContract) ERC721("Tiny Titans", "Hatchling Titans") {}

    function calcAttributeValue(uint _value) internal view returns (uint) {
        uint newValue = (_value * IConstants(constantsContract).hatchlingsPercentageOfParent()) / 100;
        if (newValue < 1) {
            newValue = 1;
        }
        return newValue;
    }

    /*
    * @dev
    * After a migration, make sure to pull the countHatchlingsOfTitan mapping from the old contract
    */
    function createNewHatchlingFromTitan(uint _idOfParent) public {
        address titansContract = IConstants(constantsContract).titansContract();
        require(ITitans(titansContract).getTitanByIndex(_idOfParent).alive, "Not allowed to mint Hatchling with dead Titan.");
        require(ITitans(titansContract).canMintHatchling(_idOfParent), "Please wait till the the mint is allowed.");
        address statisticsContract = IConstants(constantsContract).statisticsContract();
        payByTits(IConstants(constantsContract).titsTokenForHatchlingMint());
        require(ITitans(titansContract).ownerOf(_idOfParent) == msg.sender, "The sender of the message is not the owner of the Titan with the provided id");

        Structs.Titan memory titan;
        titan = ITitans(titansContract).getTitanByIndex(_idOfParent);

        hatchlingCount = hatchlingCount + 1;

        Structs.Hatchling memory hatchling = createHatchling(
            Structs.NewHatchlingEntity(hatchlingCount, titan.id, titan.hitPoints, titan.basicDmg, titan.stamina, titan.attackPower, titan.defense, titan.initiative, titan.agility)
        );

        hatchling.parentType = "Titan";

        addressToHatchlings[msg.sender].push(hatchlingCount);
        indexToHatchling[hatchlingCount] = hatchling;

        _mint(msg.sender, hatchlingCount);
        IStatistics(statisticsContract).increaseTotalHatchlingsStat();

        ITitans(titansContract).executeMintSetting(_idOfParent);

        emit NewHatchlingMinted(hatchlingCount);
    }

    /*
    * @dev
    * After a migration, make sure to pull the countHatchlingsOfTitan mapping from the old contract
    */
    function createNewHatchlingFromCommander(uint _idOfParent) public {
        address commandersContract = IConstants(constantsContract).commandersContract();
        require(ICommanders(commandersContract).canMintHatchling(_idOfParent), "Please wait till the the mint is allowed.");
        address statisticsContract = IConstants(constantsContract).statisticsContract();
        payByTits(IConstants(constantsContract).titsTokenForHatchlingMint());
        require(ICommanders(commandersContract).ownerOf(_idOfParent) == msg.sender, "The sender of the message is not the owner of the Commander Titan with the provided id");

        Structs.Commander memory commander;
        commander = ICommanders(commandersContract).getCommanderByIndex(_idOfParent);

        hatchlingCount = hatchlingCount + 1;

        Structs.Hatchling memory hatchling = createHatchling(
            Structs.NewHatchlingEntity(hatchlingCount, commander.id, commander.hitPoints, commander.basicDmg, commander.stamina, commander.attackPower, commander.defense, commander.initiative, commander.agility)
        );
        hatchling.parentType = "Commander";

        addressToHatchlings[msg.sender].push(hatchlingCount);
        indexToHatchling[hatchlingCount] = hatchling;

        _mint(msg.sender, hatchlingCount);
        IStatistics(statisticsContract).increaseTotalHatchlingsStat();

        ICommanders(commandersContract).executeMintSetting(_idOfParent);

        emit NewHatchlingMinted(hatchlingCount);
    }

    function createHatchling(Structs.NewHatchlingEntity memory stats) internal view returns (Structs.Hatchling memory _hatchling) {
        Structs.Hatchling memory hatchling;
        hatchling.id = stats.id;
        hatchling.level = 1;
        hatchling.currentExperience = 0;
        hatchling.experienceForNextLevel = 100;

        hatchling.name = IHelpers(IConstants(constantsContract).helpersContract()).concat("Hatchling #", hatchling.id);
        hatchling.descendantOf = stats.parentId;
        hatchling.hitPoints = calcAttributeValue(stats.hitPoints);
        hatchling.basicDmg = calcAttributeValue(stats.basicDmg);
        hatchling.stamina = calcAttributeValue(stats.stamina);
        hatchling.attackPower = calcAttributeValue(stats.attackPower);
        hatchling.defense = calcAttributeValue(stats.defense);
        hatchling.initiative = calcAttributeValue(stats.initiative);
        hatchling.agility = calcAttributeValue(stats.agility);
        hatchling.luck = 1;
        hatchling.owner = payable(msg.sender);
        hatchling.fightsWon = 0;
        hatchling.fightsLost = 0;
        hatchling.exhaustion = 0;
        hatchling.lastTimeRested = block.number;
        hatchling.hunger = 0;
        hatchling.lastTimeFed = block.number;
        hatchling.alive = true;
        hatchling.price = 0;
        hatchling.powerUps = [uint(0), uint(0), uint(0), uint(0), uint(0)];
        hatchling.hasPowerUps = false;
        hatchling.restPoints = 0;
        hatchling.resCounter = 0;
        hatchling.lastSellPrice = 0;

        return hatchling;
    }

    function checkAddressHasTitan(address _address) public override view returns (bool _hasTitan) {
        return addressToHatchlings[_address].length > 0;
    }

    function getHatchlingByIndex(uint _id) public override view returns (Structs.Hatchling memory _hatchling) {
        return indexToHatchling[_id];
    }

    function getFullHatchlingByIndex(uint _id) public override view returns (Structs.Hatchling memory _hatchling) {
        _hatchling = indexToHatchling[_id];
        _hatchling.restPoints = ITamagotchi(IConstants(constantsContract).hatchlingTamagotchiContract()).getRestPoints(_id);
        _hatchling.hunger = ITamagotchi(IConstants(constantsContract).hatchlingTamagotchiContract()).getHunger(_id);
        if (_hatchling.hunger == 100) {
            _hatchling.alive = false;
        }
        return _hatchling;
    }

    function getAllTitanIDsByAddress(address _address) public override view returns (uint[] memory _id) {
        return addressToHatchlings[_address];
    }

    function checkHatchlingHasPowerUps(uint _id) public override view returns (bool _hasPowerUps) {
        return indexToHatchling[_id].hasPowerUps;
    }

    function isNameTaken(string calldata name) public override view returns (bool) {
        return nameTaken[name];
    }

    /**
    *
    * @dev
    * Retrieves the batch of titans which IDs are within the lower and upper bounds
    *
    */
    function getHatchlingsBatch(uint _lowerBound, uint _upperbound) public view returns (Structs.Hatchling[] memory _hatchling) {
        address statisticsContract = IConstants(constantsContract).statisticsContract();
        uint totalHatchlings = IStatistics(statisticsContract).totalHatchlingsStat();
        require(_lowerBound > 0 && _lowerBound <= _upperbound && _upperbound <= totalHatchlings, "Invalid set of bounds provided.");
        Structs.Hatchling[] memory hatchlingArray = new Structs.Hatchling[](_upperbound - _lowerBound + 1);
        uint arrayCounter = 0;
        while (_lowerBound <= _upperbound) {
            Structs.Hatchling memory hatchling = getFullHatchlingByIndex(_lowerBound);
            hatchlingArray[arrayCounter] = hatchling;
            _lowerBound++;
            arrayCounter++;
        }

        return hatchlingArray;
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://tinytitans.fi/titanMetadata/hatchlings/";
    }

    function renameHatchling(uint _id, string calldata _newName) public {
        require(msg.sender == ownerOf(_id), "Sender is not owner.");
        require(!nameTaken[_newName], "Hatchling name already taken.");
        payByTits(IConstants(constantsContract).titsTokenForRenameTitan());
        indexToHatchling[_id].name = IHelpers(IConstants(constantsContract).helpersContract()).limitNameLength(_newName);
        emit HatchlingRenamed(_id, _newName);
    }

    function executeLevelUp(uint _id, uint[11] memory _newStats) public override onlyAllowedContracts {
        indexToHatchling[_id].level = _newStats[0];
        indexToHatchling[_id].currentExperience = _newStats[1];
        indexToHatchling[_id].experienceForNextLevel = _newStats[2];
        indexToHatchling[_id].hitPoints = _newStats[3];
        indexToHatchling[_id].basicDmg = _newStats[4];
        indexToHatchling[_id].stamina = _newStats[5];
        indexToHatchling[_id].attackPower = _newStats[6];
        indexToHatchling[_id].defense = _newStats[7];
        indexToHatchling[_id].initiative = _newStats[8];
        indexToHatchling[_id].agility = _newStats[9];
        indexToHatchling[_id].luck = _newStats[10];

        emit ExecutedLevelUp(_id, _newStats[0]);
    }

    function executeFeed(uint _id) public override onlyAllowedContracts {
        indexToHatchling[_id].hunger = 0;
        indexToHatchling[_id].lastTimeFed = block.number;
        emit ExecutedFeed(_id, indexToHatchling[_id].lastTimeFed);
    }

    function executeRest(uint _id, uint _restPoints) public override onlyAllowedContracts {
        uint256 exhaustion = indexToHatchling[_id].exhaustion;

        if (_restPoints > exhaustion) {
            indexToHatchling[_id].exhaustion = 0;
        } else {
            indexToHatchling[_id].exhaustion = exhaustion - _restPoints;
        }
        indexToHatchling[_id].lastTimeRested = block.number;
        emit ExecutedRest(_id, indexToHatchling[_id].lastTimeRested);
    }

    function executeResurrect(uint _id) public override onlyAllowedContracts {
        indexToHatchling[_id].hunger = 0;
        indexToHatchling[_id].lastTimeFed = block.number;
        indexToHatchling[_id].alive = true;
        indexToHatchling[_id].resCounter = indexToHatchling[_id].resCounter + 1;
        emit ExecutedResurrect(_id);
    }

    function executeAliveChange(uint _id, bool _aliveStatus) public override onlyAllowedContracts {
        indexToHatchling[_id].alive = _aliveStatus;
        emit ExecutedAliveChange(_id, _aliveStatus);
    }

    function executePowerUpChange(uint _id, uint[5] memory _powerUps, bool _powerUpStatus) public override onlyAllowedContracts {
        indexToHatchling[_id].powerUps = _powerUps;
        indexToHatchling[_id].hasPowerUps = _powerUpStatus;
        emit ExecutedPowerUpChange(_id);
    }

    function executeExhaustionChange(uint _id, uint _exhaustionToAdd) public override onlyAllowedContracts {
        uint256 exhaustion = indexToHatchling[_id].exhaustion;

        if (exhaustion + _exhaustionToAdd > 100) {
            indexToHatchling[_id].exhaustion = 100;
        } else {
            indexToHatchling[_id].exhaustion = exhaustion + _exhaustionToAdd;
        }
        emit ExecutedExhaustionChange(_id, indexToHatchling[_id].exhaustion);
    }

    function executeWinningStatChange(uint _id, uint _xp) public override onlyAllowedContracts {
        indexToHatchling[_id].fightsWon = indexToHatchling[_id].fightsWon + 1;
        indexToHatchling[_id].currentExperience = _xp;
        emit ExecutedWinningStat(_id);
    }

    function executeLosingStatChange(uint _id, uint _xp) public override onlyAllowedContracts {
        indexToHatchling[_id].fightsLost = indexToHatchling[_id].fightsLost + 1;
        indexToHatchling[_id].currentExperience = _xp;
        emit ExecutedLosingStat(_id);
    }

    function migrateHatchling(Structs.Hatchling calldata hatchling) public override onlyAllowedContracts {
        addressToHatchlings[hatchling.owner].push(hatchling.id);
        indexToHatchling[hatchling.id] = hatchling;
        nameTaken[hatchling.name] = true;
        _mint(hatchling.owner, hatchling.id);
        hatchlingCount = hatchlingCount + 1;
        emit ExecutedMigrateHatchling(hatchling.id);
    }

}

// File: /contracts/structs/Structs.sol
// SPDX-License-Identifier: TINY TITANS

pragma solidity ^0.8.9;

library Structs {

    struct Titan {
        uint id;
        uint level;
        uint currentExperience;
        uint experienceForNextLevel;
        string name;
        uint hitPoints;
        uint basicDmg;
        uint stamina;
        uint attackPower;
        uint defense;
        uint initiative;
        uint agility;
        uint luck;
        address payable owner;
        uint fightsWon;
        uint fightsLost;
        uint exhaustion;
        uint lastTimeRested;
        uint hunger;
        uint lastTimeFed;
        bool alive;
        uint price;
        uint[5] powerUps;
        bool hasPowerUps;
        uint restPoints;
        uint resCounter;
        uint lastSellPrice;
        uint nextPossibleMint;
        uint mintedHatchlings;
    }

    struct Commander {
        uint id;
        uint level;
        string name;
        uint hitPoints;
        uint basicDmg;
        uint stamina;
        uint attackPower;
        uint defense;
        uint initiative;
        uint agility;
        address payable owner;
        uint fightsWon;
        uint fightsLost;
        uint price;
        uint lastSellPrice;
        uint nextPossibleMint;
        uint mintedHatchlings;
    }

    struct Hatchling {
        uint id;
        uint level;
        uint currentExperience;
        uint experienceForNextLevel;
        string name;
        uint descendantOf;
        string parentType;
        uint hitPoints;
        uint basicDmg;
        uint stamina;
        uint attackPower;
        uint defense;
        uint initiative;
        uint agility;
        uint luck;
        address payable owner;
        uint fightsWon;
        uint fightsLost;
        uint exhaustion;
        uint lastTimeRested;
        uint hunger;
        uint lastTimeFed;
        bool alive;
        uint price;
        uint[5] powerUps;
        bool hasPowerUps;
        uint restPoints;
        uint resCounter;
        uint lastSellPrice;
    }

    struct UserAccountStruct {
        uint titsBalance;
        Titan[] titanArray;
        Commander[] commanderArray;
        Hatchling[] hatchlingArray;
    }

    struct NewHatchlingEntity {
        uint id;
        uint parentId;
        uint hitPoints;
        uint basicDmg;
        uint stamina;
        uint attackPower;
        uint defense;
        uint initiative;
        uint agility;
    }

    struct PowerUps {
        uint percStam;
        uint percAp;
        uint percDef;
        uint percInit;
        uint percAgi;
    }

    struct FightEntity {
        uint id;
        address owner;
        uint currentExperience;
        uint experienceForNextLevel;
        uint hitPoints;
        uint basicDmg;
        uint stamina;
        uint attackPower;
        uint defense;
        uint initiative;
        uint agility;
    }
}

