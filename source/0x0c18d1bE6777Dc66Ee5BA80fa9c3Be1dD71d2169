Months==0){\r\n             if(startMonthIsClaim\u003e0 || curMonthIsClaim\u003e0){\r\n                 totalCycleTimes=1;\r\n             }\r\n         }\r\n         \r\n        uint256 subCycleTimes=totalCycleTimes-tokenRelease.historyCycleTimes;\r\n        if(subCycleTimes\u003e0){\r\n           \r\n            if(subCycleTimes\u003etokenRelease.pendingCycleTimes){\r\n                subCycleTimes=tokenRelease.pendingCycleTimes;\r\n            }\r\n            cycleTimes=subCycleTimes;\r\n            amount=tokenRelease.totalAmount.div(tokenRelease.totalCycleTimes).mul(cycleTimes);\r\n        }\r\n        \r\n        return (cycleTimes, amount);\r\n    }\r\n    \r\n     /**\r\n     * @dev Returns the cap on the token\u0027s total supply.\r\n     */\r\n    function getMaxSupply() public view returns (uint256) {\r\n        return _maxSupply;\r\n    }\r\n    \r\n    // Require that the total supply does not become higher than the maxSupply when minting, minting will \r\n    // only happen once as there\u0027s no public mint function to be called\r\n    function _mint(address account, uint256 amount) internal virtual override {\r\n        require(totalSupply() + amount \u003c= getMaxSupply(), \"ERC20Capped: cap exceeded\");\r\n        super._mint(account, amount);\r\n    }\r\n    \r\n    function toWei(uint256 amount) private view returns(uint256) {\r\n        return amount*(uint256(10) ** _decimals);\r\n    }\r\n    \r\n}"}}