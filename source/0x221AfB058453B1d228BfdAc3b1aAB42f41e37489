        emit SetBuyBackFee(_value);
    }


    // get total fee
    function getTotalFee() public view returns(uint256){

        uint256 fee = 0;

        if(isLiquidityEnabled){ fee += liquidityFee; }
        if(isDevAndMarketingEnabled){ fee += devAndMarketingFee; }
        if(isBuyBackEnabled){ fee += buyBackFee; }
        if(isCharityEnabled){ fee += charityFee; }
        
        return fee;
    } //end get total fee


    /**
     * @dev toggle swap and add liquidity mode
     */
    modifier lockSwapAndLiquify {
        isSwapAndLiquifyLocked = true;
        _;
        isSwapAndLiquifyLocked = false;
    }


    function setV2UniswapRouter(address _uniswapV2Contract) public onlyOwner() {
        uniswapV2Router = IUniswapV2Router02(_uniswapV2Contract);
    } 


    /**
    * @dev lets swap token for bnb
    */
    function swapTokenForBNB(uint256 _tokenAmount) private returns(uint256) {

        address[] memory path = new address[](2);

        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), _tokenAmount);

        uint256  bnbCurrentBalance = address(this).balance;

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            _tokenAmount,
            0, // accept any amoun
            path,
            address(this),
            (block.timestamp + 10)
        );        

        return uint256(address(this).balance.sub(bnbCurrentBalance,"subtraction error swapTokenForBNB::balance.sub(bnbCurrentBalance)"));
    } //end


    /**
    * @dev lets swap token for bnb   lockSwapAndLiquify
    */
    function swapBNBForTokens(uint256 amountInBNB) private  {

        address[] memory path = new address[](2);

        path[0] =  uniswapV2Router.WETH();
        path[1] = address(this); 


        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens {value: amountInBNB} (
            0, // accept any amount
            path,
            address(buybackTokenHolderContract),
            block.timestamp.add(360)
        );        


    } //end

     /**
     * @dev add liquidity
     */
    function _addLquidity(uint256 _tokenAmount, uint256 _amountInBNB) private  {
        
        _approve(address(this), address(uniswapV2Router), _tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH { value: _amountInBNB } (
            address(this), //token contract address
            _tokenAmount, // token amount to add liquidity
            0, //amountTokenMin 
            0, //amountETHMin
            liquidityOwner, //owner of the liquidity
            (block.timestamp + 15) //deadline
        );
    } //end add liquidity


    /**
     * @dev swap and add liquidity
     */
    function swapAndLiquify(uint256 _tokenAmount) private lockSwapAndLiquify {

        require(_tokenAmount > 0, "Amount cannot be 0");

        uint256 tokenAmountHalf = _tokenAmount.div(2);
        
        //lets swap to get some base asset
        uint256 swappedBNBAmount = swapTokenForBNB(tokenAmountHalf);

        _addLquidity(tokenAmountHalf, swappedBNBAmount);

    } //end 
    

    /**
     * add lets add initial liquidity
     */
    function addInitialLiquidity(uint256 _tokenAmount ) external payable onlyOwner lockSwapAndLiquify  {
        
        require(!_isInitialized,"Method Alerady Initialised");
        require(msg.value > 0, "BNB amount should be greater than 0");
        require(_tokenAmount > 0,"_tokenAmount should be greater than 0");

        //excludeFromFees(_msgSender());

        require(address(this).send(msg.value), "Failed to transfer bnb to contract");

        //put token to contract
        super._transfer(_msgSender(), address(this), _tokenAmount);

        _approve(address(this), address(uniswapV2Router), _tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH { value: msg.value } (
            address(this), //token contract address
            _tokenAmount, // token amount we wish to provide liquidity for
            _tokenAmount, //amountTokenMin 
            msg.value, //amountETHMin
            _msgSender(), 
            (block.timestamp + 1000) //deadline
        );

        _isInitialized = true;
    } //end add liquidity
    

    function excludeAccountFromFees(address _account) public onlyOwner() returns(bool){
        excludedFromFees[_account] = true;
        return true;
    }

    function removeExcludedFromFees(address _account) public onlyOwner() returns(bool){ 
        delete excludedFromFees[_account];
        return true;
    }

    function isExcludedFromFees(address _account) public view returns(bool){
        return excludedFromFees[_account];
    }


    /**
     * setRewardStartDelay
     */
    function setRewardStartDelay(uint256 _timeInSeconds) public onlyOwner {
        rewardStartDelay = _timeInSeconds;
        emit  SetRewardStartDelay(_timeInSeconds);
    }

    /**
     * set min expected holdl period
     */
    function setMinExpectedHoldlPeriod(uint256 _timeInSeconds) public onlyOwner {

        require(_timeInSeconds > rewardStartDelay,  
            string(abi.encodePacked("timestamp cannot be less than rewardStartDelay: ", " ", bytes32(rewardStartDelay)))
        );

        minExpectedHoldlPeriod = _timeInSeconds;

        emit SetMinExpectedHoldlPeriod(_timeInSeconds);
    } //end fun 

    /**
     * set the minimum amount before buying back with bnb
     */
    function setMinAmountBeforeBuyingBackTokens(uint256 amountInBNB) public onlyOwner {
        minAmountBeforeBuyingBack = amountInBNB;
        emit SetMinAmountBeforeBuyingBackTokens(amountInBNB);
    }

    /**
     * minimum amount before selling the tokens for buyback
     */
    function setMinAmountBeforeSellingTokensForBuyBack(uint256 amountInTokens) public onlyOwner {
        minAmountBeforeSellingTokensForBuyBack = amountInTokens;
        emit SetMinAmountBeforeSellingTokensForBuyBack(amountInTokens);
    }

    /**
     * minimum amount of tokens before selling tokens to add liquidity
     */
    function setMinAmountOfTokensBeforeSellingToAddLiuqidity(uint256 amountInTokens) public onlyOwner {
        minAmountOfTokensBeforeSellingToAddLiuqidity = amountInTokens;
        emit SetMinAmountOfTokensBeforeSellingToAddLiuqidity(amountInTokens);
    }

    
    /**
     * set buyback bnb slipper divisor
     */
    function setBuyBackBNBSplitDivisor(uint256 _value) public onlyOwner {
        
        buyBackBNBSplitDivisor = _value;
        emit SetBuyBackBNBSplitDivisor(_value);

    } //end function

    /**
     * get reward
     */
    function getReward(address _account) public view returns(uint256) {

        //lets get account balance
        uint256 accountRealBalance = _balances[_account];

    
        if(accountRealBalance == 0) {
            //console.log("HoldlersReward: Zero Balance");
            return 0;
        }
        
        //lets get account deposits info
        AccountInfoStruct memory _accountInfo = _accountsInfoMap[_account];

        if(_accountInfo.depositCount == 0 || _accountInfo.timestampSum == 0){
            return 0;
        }

        //lets get the deposit timestamp average
        uint256 depositTimestampAverage = _accountInfo.timestampSum.div(_accountInfo.depositCount);

        uint256 accountHoldlPeriod = (block.timestamp - depositTimestampAverage);

        //if the deposit average is less than 1 day, give no reward
        if(accountHoldlPeriod < rewardStartDelay) {
            //console.log("Account holdle period is less than reward start delay", rewardStartDelay, accountHoldlPeriod);
            return 0;
        } //end if 
        

        //lets check how many percent the account has holdled against the expected min hol period
        uint256 percentHoldledAgainstExpected = (accountHoldlPeriod.mul(100)).div(minExpectedHoldlPeriod);

        //lets capp it to 100% 
        if(percentHoldledAgainstExpected > 100){ percentHoldledAgainstExpected = 100; }

        uint256 balanceBasedReward = (accountRealBalance.mul(holdlersRewardPool)).div(totalSupply());

              
        // balanceBasedReward based on holdled duration or time
        // The time based reward checks against users moving balance to different addresses to take rewards
        uint256 rewardBasedOnTimeAndBalance = (percentHoldledAgainstExpected.mul(balanceBasedReward)).div(100);

        //now lets compare last released reward, if its greater than current reward 
        // then we will return 0 else we will get substration error 
        if(_accountInfo.totalRewardReleased >= rewardBasedOnTimeAndBalance) {
            return 0;
        }

        //lets deduct rewardBasedOnTimeAndBalance from _accountInfo.totalReleasedRewards,
        // this will prevent user from tricking the system to receive more rewards as
        // the rewards are released on every withdrawal 

        uint256 finalReward = rewardBasedOnTimeAndBalance.sub(_accountInfo.totalRewardReleased,"Subtraction error getReward:: rewardBasedOnTimeAndBalance.sub");

        //lets make negative numbers 0 as we dont want it to reduce user balance
        if(finalReward < 0){
            finalReward = 0;
        }

        return finalReward;
    }//end fun 


    /**
     * _transfer
     */
    function _transfer(address sender, address recipient, uint256 amount) override internal virtual {
        
        
        require(amount > 0, "Amount cannot be less than 0");
        require(_balances[sender] >= amount, "Insufficient balance");

        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");

        uint256 amountToTransfer = _preProcessTransfer(sender, recipient, amount);

        _balances[sender] = _balances[sender].sub(amountToTransfer, "ERC20: transfer amount exceeds balance");
        _balances[recipient] = _balances[recipient].add(amountToTransfer);

        // update holders info
        updateHoldlerAccountsInfo(sender, recipient);

        emit Transfer(sender, recipient, amountToTransfer);

    } //end fun 
    
    
    /**
     * pre process transfer 
     */
    function _preProcessTransfer(address sender, address recipient, uint256 amount) private returns(uint256) {


        if(isExcludedFromFees(sender) || 
           isExcludedFromFees(recipient) || 
           isSwapAndLiquifyLocked ||
           getTotalFee() == 0 ||
           sender == address(uniswapV2Router)  // this is needed for receiving removed user liquidity
        ){
            return amount;
        }
        

        // before we process anything, lets release senders reward
        releaseAccountReward(sender);
        

        //lets get totalTax to deduct
        uint256 totalFeeToDeduct =  computePecentToAmount(getTotalFee(), amount);

        //lets substract the fee to system account
         internalTokenTransfer(sender, address(this), totalFeeToDeduct, false);

        uint256 amountWithFee = amount.sub(totalFeeToDeduct);

        //lets process holdlers reward
        if(isHoldlersRewardEnabled && holdlersRewardFee > 0){
            holdlersRewardPool = holdlersRewardPool.add(computePecentToAmount(holdlersRewardFee, amount));
        } //end if 

        //lets process dev and marketing 
        if(isDevAndMarketingEnabled && devAndMarketingFee > 0) {
            
            uint256 devAndMarketingAmount = computePecentToAmount(devAndMarketingFee, amount);

            //console.log("devAndMarketingAmount ===>>>", devAndMarketingAmount);

            internalTokenTransfer(address(this),devAndMarketingWallet, devAndMarketingAmount, true);

        } //end process dev and marketing

        // lets process  charity data
        if(isCharityEnabled && charityFee >= 0) {
            
            uint256 charityAmount = computePecentToAmount(charityFee, amount);

            //console.log("charityAmount ===>>>", charityAmount);

            internalTokenTransfer(address(this),charityWallet, charityAmount, true);

        } //end if 

        //process liquidity
        if(isLiquidityEnabled && liquidityFee > 0){
            
            totalAmountToLiquidify = totalAmountToLiquidify.add( computePecentToAmount(liquidityFee, amount) );

            //console.log("totalAmountToLiquidify ===>>>", totalAmountToLiquidify);
            
            //if the sender is not uniswap pair then add liquidity, this is necessary else trade wont work in uniswap code
            if(sender != uniswapV2Pair) {

                //store balance
                uint256 amounToLiquidify = totalAmountToLiquidify;

                if(amounToLiquidify >= minAmountOfTokensBeforeSellingToAddLiuqidity){
                    
                     //lets swap and provide liquidity
                    swapAndLiquify(amounToLiquidify);

                    totalAmountToLiquidify = totalAmountToLiquidify.sub(amounToLiquidify, "subtraction error _preProcessTransfer::totalAmountToLiquidify");
                } //end if

            } //end if 

        } //end if auto liquidity is on

        
        // lets process buyBack
        if(isBuyBackEnabled && buyBackFee > 0) {

            buyBackTokenPool = buyBackTokenPool.add( computePecentToAmount(buyBackFee, amount) );

            if(buyBackTokenPool >= minAmountBeforeSellingTokensForBuyBack && sender != uniswapV2Pair) {
                
                uint256 buyBackTokenSwapAmount = buyBackTokenPool;

                uint256 returnedBNBValue = swapTokenForBNB(buyBackTokenSwapAmount);

                buyBackBNBPool = buyBackBNBPool.add(returnedBNBValue);

                buyBackTokenPool = buyBackTokenPool.sub(buyBackTokenSwapAmount, "subtraction error buyBackTokenPool buyback sub");

            } ///end if 

            
            console.log("Sender ===>>>", sender);
            console.log("recipient ===>>>", recipient);
            console.log("uniswapV2Pair ===>>>", uniswapV2Pair);

            // lets check if buyback is possible
            if(buyBackBNBPool > minAmountBeforeBuyingBack && sender != uniswapV2Pair) {

                uint256 bnbAmountToSell = buyBackBNBPool;

             
                // uint256 swappedTokensAmount =
                swapBNBForTokens(bnbAmountToSell.div(buyBackBNBSplitDivisor));

                uint256 buyBackSwappedTokens = buybackTokenHolderContract.moveTokensToContract();
                
                //console.log("BuyBack Success Amount ====>> ", swappedTokensAmount);

                //if the swapped token is greater than 0
                if(buyBackSwappedTokens > 0){

                    uint256 swappedTokensHalf = buyBackSwappedTokens.div(2);

                    console.log("swappedTokensAmountsHalf ===>>", swappedTokensHalf);

                    //add half of the tokens to holders reward pool
                    holdlersRewardPool = holdlersRewardPool.add(swappedTokensHalf);

                    //lets burn the half
                    //buyBackTokenPool = buyBackTokenPool.add(swappedTokensHalf);
                    internalTokenTransfer(address(this), address(0), swappedTokensHalf, true);
                }

                //lets subtract the swapped bnb from the pool
                buyBackBNBPool = buyBackBNBPool.sub(bnbAmountToSell);
            } //end if 
            
        } //end if


        return amountWithFee;

    } //end fun


    /**
     * moveTokens 
     */
    function internalTokenTransfer(address _from, address _to, uint256 _amount, bool triggerEvent) private {
        
        _balances[_from]   =  _balances[_from].sub(_amount);
        _balances[_to]     =  _balances[_to].add(_amount);

        if(triggerEvent){
            emit Transfer(_from, _to, _amount);
        }
    }

    /**
    * processing holdling rewards
    */
    function updateHoldlerAccountsInfo(address sender, address recipient ) private {

        AccountInfoStruct memory recipientAccountInfo = _accountsInfoMap[recipient];

        //since deposits marks accumulation and holdling of more assets, lets process for user
        _accountsInfoMap[recipient].depositCount = recipientAccountInfo.depositCount.add(1);
        _accountsInfoMap[recipient].timestampSum = recipientAccountInfo.timestampSum.add(block.timestamp);
        
        
        //lets process global deposit info 
        _accountsInfoAggregate.depositCount = _accountsInfoAggregate.depositCount.add(1);
        _accountsInfoAggregate.timestampSum = _accountsInfoAggregate.depositCount.add(block.timestamp);

        //if sender has no more tokens, lets remove his or data
        if(_balances[sender] == 0){
            
            AccountInfoStruct memory senderAccountInfo = _accountsInfoMap[sender];

            //avoid overflows with this
            if(_accountsInfoAggregate.timestampSum <= senderAccountInfo.timestampSum){
                _accountsInfoAggregate.timestampSum = 0;
            } else {
                _accountsInfoAggregate.timestampSum = _accountsInfoAggregate.timestampSum.sub(senderAccountInfo.timestampSum,"Subtraction error:: updateHoldlerAccountsInfo _accountsInfoAggregate.timestampSum");
            }

            // avoid overflow, if the _accountsInfoAggregate.depositCount is less than the value to sub
            if( _accountsInfoAggregate.depositCount <= senderAccountInfo.depositCount) {
                _accountsInfoAggregate.depositCount =  0;
            } else {
                _accountsInfoAggregate.depositCount =  _accountsInfoAggregate.depositCount.sub(senderAccountInfo.depositCount, "Subtraction error:: updateHoldlerAccountsInfo accountsInfoAggregate.depositCount");
            }

            //user is no more holdling so we remove it
            delete _accountsInfoMap[sender];

        }//end if 

    } //end process holdlEffect

    /**
     * processRewardReleaseOnTransfer
     */
    function releaseAccountReward(address _account) private {

        //lets release sender's reward
        uint256 accountReward = getReward(_account);

        //dont bother processing if balance is 0
        if(accountReward <= 0) {
            return;
        } //end if 

        _balances[_account] = _balances[_account].add(accountReward);

        //lets get account info 
        _accountsInfoMap[_account].totalRewardReleased =  _accountsInfoMap[_account].totalRewardReleased.add(accountReward);

        // lets substract the received rewards from the total reward pool
        if(accountReward >= holdlersRewardPool) {
            //substract the amount taken from the pool
            holdlersRewardPool = 0;
        } else {
           holdlersRewardPool =  holdlersRewardPool.sub(accountReward);
        } //end if

        emit RewardReleased(_account,accountReward);
    } //end function


    /**
    * @dev calcultae percentages
    */
    function computePecentToAmount(uint256 _percentInBps, uint256 _amount) private pure returns(uint256) {
        //to get pbs,multiply percentage by 100
        return  (_amount.mul(_percentInBps)).div(10_000);
    }

    
    function transferOtherTokens(address _contractAddress, address _toAddress) public onlyOwner {
        
        ERC20 erc20Token  =  ERC20(_contractAddress);

        uint256 balance = erc20Token.balanceOf(address(this));
        
        if(balance > 0){
            erc20Token.transfer(_toAddress, balance);
        }
    } //end function


    /**
     * move Ethers
     */
    function moveEthers(address payable _toAddress) public onlyOwner {
        uint256 balance = address(this).balance;

        if(balance > 0){
            _toAddress.transfer(balance);
        }
    }

}//end contract