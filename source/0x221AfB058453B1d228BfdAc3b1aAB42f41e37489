	}

	function log(address p0, address p1, address p2, bool p3) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,bool)", p0, p1, p2, p3));
	}

	function log(address p0, address p1, address p2, address p3) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,address)", p0, p1, p2, p3));
	}

}

// File: contracts/BuyBackTokenHolder.sol


pragma solidity ^0.7.6;
pragma abicoder v2;

interface ERC20_I {
    function transfer(address to, uint value) external returns (bool);
    function balanceOf(address owner) external view returns (uint);
    function approve(address spender, uint value) external returns (bool);
}

contract BuyBackTokenHolder  {

    event _Receive(address _sender, uint256 _amount);

    address public tokenContractAddress;

    constructor(address _tokenContractAddress) {
        tokenContractAddress = _tokenContractAddress;
    }

    modifier onlyTokenContract {
        require(msg.sender == tokenContractAddress, "Only Token Contract is Permitted");
        _;
    }

    receive () external payable {
         emit _Receive(msg.sender,msg.value);
    }

    fallback () external payable {}


    function moveTokensToContract() public onlyTokenContract returns(uint256) {

        ERC20_I tokenContract = ERC20_I(tokenContractAddress);

        uint256 tokenBalance = tokenContract.balanceOf(address(this));

        tokenContract.transfer(tokenContractAddress, tokenBalance);

        return tokenBalance;
    }

}

// File: contracts/CanadaCoin.sol


pragma solidity ^0.7.6;






contract CanadaCoin is   Context, Ownable, ERC20, ERC20Burnable {

    event _Receive(address _sender, uint256 _amount);
    event Burn(address indexed account, uint256 amount);
    event SetDevAndMarketingWallet(address indexed account);
    event SetCharityWallet(address indexed account);
    event SetLiquidityOwner(address indexed _wallet);
    event EnableLiquidity(bool _option);
    event EnableCharity(bool _option);
    event EnableDevAndMarketing(bool _option);
    event EnableBuyBack(bool _option);
    event SetDevAndMarketingFee(uint256 _value);
    event SetCharityFee(uint256 _value);
    event SetBuyBackFee(uint256 _value);
    event RewardReleased(address indexed _account, uint256 _reward);
    event SetRewardStartDelay(uint256 _timeInSeconds);
    event SetMinExpectedHoldlPeriod(uint256 _timeInSeconds);
    event EnableHodlersReward(bool _option);
    event SetLiquidityFee(uint256 _value);
    event SetHoldlersRewardFee(uint256 _value);
    event SetMinAmountBeforeBuyingBackTokens(uint256 amountInBNB);
    event SetMinAmountBeforeSellingTokensForBuyBack(uint256 amountInTokens);
    event SetMinAmountOfTokensBeforeSellingToAddLiuqidity(uint256 amountInTokens);
    event SetBuyBackBNBSplitDivisor(uint256 _value);

    using SafeMath for uint256;

    struct AccountInfoStruct {
        uint256 depositCount;
        uint256 timestampSum;
        uint256 totalRewardReleased;
    }

    string  constant  _tokenName = "Canada Coin";
    string  constant  _tokenSymbol = "CADA";
    uint8   constant  _tokenDecimals = 9;
    uint256 constant  _tokenSupply = 1_000_000_000_000_000 * (10 ** _tokenDecimals); // 100b

    bool public _isInitialized = false;

    //set options
    bool public isHoldlersRewardEnabled = true;
    bool public isLiquidityEnabled = true;
    bool public isDevAndMarketingEnabled = true;
    bool public isCharityEnabled = true;
    bool public isBuyBackEnabled = true;

    //set fee rates
    uint256 public holdlersRewardFee = 400;
    uint256 public liquidityFee = 300; 
    uint256 public devAndMarketingFee = 150;
    uint256 public charityFee = 150;
    uint256 public buyBackFee = 200;

    // re-entrancy guard var
    bool isSwapAndLiquifyLocked = false;

    //exclude from fees
    mapping(address => bool) private excludedFromFees;

    //exclude an account from rewards reward
    mapping(address => bool) private excludedFromRewards;

    IUniswapV2Router02 public  uniswapV2Router;

    address public  uniswapV2Pair;

    address payable devAndMarketingWallet = 0x6DE1617Db4ab092f1E2832a61dCE7BBd45B5B86E;

    address payable charityWallet = 0x8a290fA91143917BeeFCa2EFc2c914a675355091;

    address liquidityOwner;

    //liquidity fund
    uint256 public totalAmountToLiquidify;

    // min amount before selling to add liquidity
    uint256 public minAmountOfTokensBeforeSellingToAddLiuqidity = 100_000_000  * (10 ** _tokenDecimals);

    uint256 public buyBackTokenPool;
    uint256 public buyBackBNBPool;
    uint256 public minAmountBeforeSellingTokensForBuyBack = 100_000_000  * (10 ** _tokenDecimals);
    uint256 public minAmountBeforeBuyingBack = 1 ether; //1 ether;
    uint256 public buyBackBNBSplitDivisor = 3;

    //fund pool
    uint256 public holdlersRewardPool;

    //acounts deposit info
    mapping(address => AccountInfoStruct) private _accountsInfoMap;

    //aggregate of all deposits info
    AccountInfoStruct _accountsInfoAggregate;


    // reward reflection delay, for security reason, a delay before account rewarding starts
    uint256 public rewardStartDelay = 48 hours;

    //minHoldPeriod
    // the expected minimum holding period, during this period, the reward will be released gradually till the min hold 
    // period is reached, once the min hold period is reached, full reward will be released as the reward pool increases
    uint256 public minExpectedHoldlPeriod = 30 days;


    //get chain id
    function getChainId() private pure returns (uint256) {
        uint256 id;
        assembly { id := chainid() }
        return id;
    }


    // buyback Token holders, uniswap forbides buying back to same contract, so this will hold 
    // the tokens which will be pulled back 
    BuyBackTokenHolder public buybackTokenHolderContract;

    constructor() ERC20(_tokenName,_tokenSymbol) {

        // lets deploy bauyback token holder contract 
        buybackTokenHolderContract = new BuyBackTokenHolder(address(this));

        _setupDecimals(_tokenDecimals);
        
        _mint(msg.sender, _tokenSupply);

        excludedFromFees[address(this)] = true;

        excludedFromFees[msg.sender] = true;

        excludedFromFees[address(buybackTokenHolderContract)] = true;

        liquidityOwner = msg.sender;
        

        uint256 chainId = getChainId();

        if(chainId == 1 || chainId == 3 || chainId == 42 || chainId == 31337){ //ethereum based chains (uniswap)
            
            uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        
        } else if (chainId == 97){ // bsc testnet pancake testnet
            
            uniswapV2Router = IUniswapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
        
        } else if(chainId == 56) { // bsc main net pancake mainnet 
           
            uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
       
        } else {
            revert("ERC20: Unsupported chain Id");
        }

        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(address(this), uniswapV2Router.WETH());
    }


    /**
     * realBalanceOf - get account real balance
     */
    function realBalanceOf(address _account)  public view returns(uint256) {
        return super.balanceOf(_account);
    }

    /**
     * override balanceOf - This now returns user balance + reward
     */
    function balanceOf(address _account) override public view returns(uint256) {

        uint256 accountBalance = super.balanceOf(_account);

        if(accountBalance <= 0){
            accountBalance = 0;
        }
        
        uint256 reward = getReward(_account);

        if(reward <= 0){
            return accountBalance;
        }

        return (accountBalance.add(reward));
    } //end fun
    



    receive () external payable {
         emit _Receive(msg.sender,msg.value);
    }

    fallback () external payable {}


    //disable all fees
