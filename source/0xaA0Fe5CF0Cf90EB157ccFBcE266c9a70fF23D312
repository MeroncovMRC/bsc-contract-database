        // 5: Partnerships/Growth  7.00% - 70,000,000 - 72 months @ rate of 1.38% per month 
        vestingTypes.push(VestingType(1380000000000000000, 0 days, false, 700, 0));

        // 6: Ecosystem  30.00% - 300,000,000 - 72 months @ 1.38% per month
        vestingTypes.push(VestingType(1380000000000000000, 0 days, false, 3000, 0));
