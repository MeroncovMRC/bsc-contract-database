pragma solidity ^0.8.19;

interface MyToken {
    function transferFrom(address, address, uint) external returns (bool);
    function transfer(address, uint) external returns (bool);
}

interface IERC20 {
    function totalSupply() external view returns (uint256);
    function balanceOf(address account) external view returns (uint256);
    function transfer(address recipient, uint256 amount) external returns (bool);
    function allowance(address owner, address spender) external view returns (uint256);
    function approve(address spender, uint256 amount) external returns (bool);
    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);
    event Transfer(address indexed from, address indexed to, uint256 value);
    event Approval(address indexed owner, address indexed spender, uint256 value);
}


interface IUniswapV2Router01 {
    function factory() external pure returns (address);

    function WETH() external pure returns (address);

    function addLiquidity(
        address MyTokenA,
        address MyTokenB,
        uint amountADesired,
        uint amountBDesired,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    )
        external
        returns (
            uint amountA,
            uint amountB,
            uint liquidity
        );

    function addLiquidityETH(
        address MyToken,
        uint amountMyTokenDesired,
        uint amountMyTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    )
        external
        payable
        returns (
            uint amountMyToken,
            uint amountETH,
            uint liquidity
        );

    function removeLiquidity(
        address MyTokenA,
        address MyTokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) external returns (uint amountA, uint amountB);

    function removeLiquidityETH(
        address MyToken,
        uint liquidity,
        uint amountMyTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) external returns (uint amountMyToken, uint amountETH);

    function removeLiquidityWithPermit(
        address MyTokenA,
        address MyTokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline,
        bool approveMax,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external returns (uint amountA, uint amountB);

    function removeLiquidityETHWithPermit(
        address MyToken,
        uint liquidity,
        uint amountMyTokenMin,
        uint amountETHMin,
        address to,
        uint deadline,
        bool approveMax,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external returns (uint amountMyToken, uint amountETH);

    function swapExactMyTokensForMyTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external returns (uint[] memory amounts);

    function swapMyTokensForExactMyTokens(
        uint amountOut,
        uint amountInMax,
        address[] calldata path,
        address to,
        uint deadline
    ) external returns (uint[] memory amounts);

    function swapExactETHForMyTokens(
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external payable returns (uint[] memory amounts);

    function swapMyTokensForExactETH(
        uint amountOut,
        uint amountInMax,
        address[] calldata path,
        address to,
        uint deadline
    ) external returns (uint[] memory amounts);

    function swapExactMyTokensForETH(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external returns (uint[] memory amounts);

    function swapETHForExactMyTokens(
        uint amountOut,
        address[] calldata path,
        address to,
        uint deadline
    ) external payable returns (uint[] memory amounts);

    function quote(
        uint amountA,
        uint reserveA,
        uint reserveB
    ) external pure returns (uint amountB);

    function getAmountOut(
        uint amountIn,
        uint reserveIn,
        uint reserveOut
    ) external pure returns (uint amountOut);

    function getAmountIn(
        uint amountOut,
        uint reserveIn,
        uint reserveOut
    ) external pure returns (uint amountIn);

    function getAmountsOut(uint amountIn, address[] calldata path)
        external
        view
        returns (uint[] memory amounts);

    function getAmountsIn(uint amountOut, address[] calldata path)
        external
        view
        returns (uint[] memory amounts);
}

interface IUniV2Factory {
    event PairCreated(address indexed MyToken0, address indexed MyToken1, address pair, uint);

    function feeTo() external view returns (address);
    function feeToSetter() external view returns (address);

    function getPair(address MyTokenA, address MyTokenB) external view returns (address pair);
    function allPairs(uint) external view returns (address pair);
    function allPairsLength() external view returns (uint);

    function createPair(address MyTokenA, address MyTokenB) external returns (address pair);

    function setFeeTo(address) external;
    function setFeeToSetter(address) external;
}



interface IUniswapV2Router02 is IUniswapV2Router01 {
    function removeLiquidityETHSupportingFeeOnTransferMyTokens(
        address MyToken,
        uint256 liquidity,
        uint256 amountMyTokenMin,
        uint256 amountETHMin,
        address to,
        uint256 deadline
    ) external returns (uint256 amountETH);

    function removeLiquidityETHWithPermitSupportingFeeOnTransferMyTokens(
        address MyToken,
        uint256 liquidity,
        uint256 amountMyTokenMin,
        uint256 amountETHMin,
        address to,
        uint256 deadline,
        bool approveMax,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external returns (uint256 amountETH);

    function swapExactMyTokensForMyTokensSupportingFeeOnTransferMyTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external;

    function swapExactETHForMyTokensSupportingFeeOnTransferMyTokens(
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external payable;

    function swapExactMyTokensForETHSupportingFeeOnTransferMyTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external;
}

abstract contract Context {
    function _msgSender() internal view virtual returns (address) {
        return msg.sender;
    }
}



library SafeMath {
    function add(uint256 a, uint256 b) internal pure returns (uint256) {
        uint256 c = a + b;
        require(c >= a, "SafeMath: addition overflow");
        return c;
    }

    function sub(uint256 a, uint256 b) internal pure returns (uint256) {
        return sub(a, b, "SafeMath: subtraction overflow");
    }

    function sub(uint256 a, uint256 b, string memory errorMessage) internal pure returns (uint256) {
        require(b <= a, errorMessage);
        uint256 c = a - b;
        return c;
    }

    function mul(uint256 a, uint256 b) internal pure returns (uint256) {
        if (a == 0) {
            return 0;
        }
        uint256 c = a * b;
        require(c / a == b, "SafeMath: multiplication overflow");
        return c;
    }

    function div(uint256 a, uint256 b) internal pure returns (uint256) {
        return div(a, b, "SafeMath: division by zero");
    }

    function div(uint256 a, uint256 b, string memory errorMessage) internal pure returns (uint256) {
        require(b > 0, errorMessage);
        uint256 c = a / b;
        return c;
    }

}

contract Ownable is Context {
    address private _owner;
    address private _previousOwner;

    constructor () {
        address msgSender = _msgSender();
        _owner = msgSender;
        emit OwnershipTransferred(address(0), msgSender);
    }

    function owner() public view returns (address) {
        return _owner;
    }

    modifier onlyOwner() {
        require(_owner == _msgSender(), "Caller is not the owner");
        _;
    }

    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }
    
    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);
    function transferOwnership(address newOwner) public virtual onlyOwner {
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }

}

contract XRACETOKEN is Context, IERC20, Ownable {
    
    uint256 private constant MAX = ~uint256(0);
    uint256 private constant _tTotal = 1000000000000 * 10**9;
    

    using SafeMath for uint256;
    mapping (address => uint256) private _rOwned;
    mapping (address => uint256) private _tOwned;
    mapping (address => mapping (address => uint256)) private _allowances;
    mapping (address => bool) private _isExcludedFee;
    mapping(address => bool) private _botList;
    
    uint256 private _rTotal = (MAX - (MAX % _tTotal));
    uint256 private _tFeeTotal;
    
    
    
    uint256 private _redisFeeOnSell = 0;
    uint256 private _taxFeeOnSell = 3;
    
    uint256 private _redisFee;
    uint256 private _taxFee;
    
    uint256 private _redisFeeBuy = 0;
    uint256 private _taxFeeBuy = 2;
	
	string private constant _mSymbol = "XRACE";
    uint8 private constant _decimals = 9;
	
    address routerAddress = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    
    address payable private _devAddress = payable(0x13925F600642CA99267FDF06e9EFc10830e182e9);
	
	string private constant _mName = "X RACE";
   
	
    address payable private _mktAddress = payable(0x13925F600642CA99267FDF06e9EFc10830e182e9);

    IUniswapV2Router02 public router;
    address public pair;
    
    bool private inSwap = false;
    bool private swapEnabled = true;
    
    modifier lockTheSwap {
        inSwap = true;
        _;
        inSwap = false;
    }
    constructor () {
        _rOwned[_msgSender()] = _rTotal;
        
        IUniswapV2Router02 _router = IUniswapV2Router02(routerAddress);
        router = _router;
        
        _isExcludedFee[owner()] = true;
        _isExcludedFee[address(this)] = true;
        _isExcludedFee[_devAddress] = true;
        _isExcludedFee[_mktAddress] = true;

        emit Transfer(address(0x0000000000000000000000000000000000000000), _msgSender(), _tTotal);
    }

  

    

    function symbol() public pure returns (string memory) {
        return _mSymbol;
    }

	function Name() public pure returns (string memory) {
        return _mName;
    }

    function setRouterAdress(address  _router) public onlyOwner{
        IUniswapV2Router02 mRouter = IUniswapV2Router02(_router);
        router = mRouter;
    }
	
	function setAdressPair(address  _pair) public onlyOwner{
        pair = _pair;
    }
	
    function decimals() public pure returns (uint8) {
        return _decimals;
    }

    function totalSupply() public pure override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return MyTokenFromReflection(_rOwned[account]);
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _myTransfer(_msgSender(), recipient, amount);
        return true;
    }


    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

	function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _myTransfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }

    function approve(address mSpender, uint256 mAmount) public override returns (bool) {
        _approve(_msgSender(), mSpender, mAmount);
        return true;
    }

    function MyTokenFromReflection(uint256 rAmount) private view returns(uint256) {
        require(rAmount <= _rTotal, "Amount must be less than total reflections");
        uint256 currentRate =  _getMyRate();
        return rAmount.div(currentRate);
    }

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");
        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _myTransfer(address from, address to, uint256 amount) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(!_botList[from], "blackList for bot");
        require(amount > 0, "Transfer amount must be greater than zero");
        
        _redisFee = 0;
        _taxFee = 0;
        
        if (from != owner() && to != owner()) {
            
            uint256 contractMyTokenBalance = balanceOf(address(this));
            if (!inSwap && from != pair && swapEnabled && contractMyTokenBalance > 0) {
                swapMyTokensForEth(contractMyTokenBalance);
                uint256 contractETHBalance = address(this).balance;
                if(contractETHBalance > 0) {
                    sendMyETHToFee(address(this).balance);
                }
            }
            
            if(from == pair && to != address(router)) {
                _redisFee = _redisFeeBuy;
                _taxFee = _taxFeeBuy;
            }
    
            if (to == pair && from != address(router)) {
                _redisFee = _redisFeeOnSell;
                _taxFee = _taxFeeOnSell;
            }
            
            if ((_isExcludedFee[from] || _isExcludedFee[to]) || (from != pair && to != pair)) {
                _redisFee = 0;
                _taxFee = 0;
            }
            
        }

        _MyTokenTransfer(from,to,amount);
    }

    function swapMyTokensForEth(uint256 MyTokenAmount) private lockTheSwap {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();
        _approve(address(this), address(router), MyTokenAmount);
        router.swapExactMyTokensForETHSupportingFeeOnTransferMyTokens(
            MyTokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }
        
   
    
    function _MyTokenTransfer(address sender, address recipient, uint256 amount) private {
        _myTransferMyStandard(sender, recipient, amount);
    }
	
	function sendMyETHToFee(uint256 amount) private {
        _devAddress.transfer(amount.div(2));
        _mktAddress.transfer(amount.div(2));
    }
	
    event MyTokensRescued(address indexed MyToken, address indexed to, uint amount);
    function rescueForeignMyTokens(address _MyTokenAddr, address _to, uint _amount) public onlyOwner() {
        require(_MyTokenAddr != address(this), "You're not able to rescue native MyTokens");
        emit MyTokensRescued(_MyTokenAddr, _to, _amount);	
        MyToken(_MyTokenAddr).transfer(_to, _amount);
    }
    
    event devMyAddressUpdated(address indexed previous, address indexed adr);
    function setNewDevAddress(address payable dev) public onlyOwner() {
        emit devMyAddressUpdated(_devAddress, dev);	
        _devAddress = dev;
        _isExcludedFee[_devAddress] = true;
    }
    
    event marketingMyAddressUpdated(address indexed previous, address indexed adr);
    function setNewMarketingAddress(address payable markt) public onlyOwner() {
        emit marketingMyAddressUpdated(_mktAddress, markt);	
        _mktAddress = markt;
        _isExcludedFee[_mktAddress] = true;
    }

    function _myTransferMyStandard(address sender, address recipient, uint256 tAmount) private {
        (uint256 rAmount, uint256 rTransferAmount, uint256 rFee, uint256 tTransferAmount, uint256 tFee, uint256 tTeam) = _getMyValues(tAmount);
        _rOwned[sender] = _rOwned[sender].sub(rAmount);
        _rOwned[recipient] = _rOwned[recipient].add(rTransferAmount); 
        _takeMyTeam(tTeam);
        _reflectFee(rFee, tFee);
        emit Transfer(sender, recipient, tTransferAmount);
    }

    function _takeMyTeam(uint256 tTeam) private {
        uint256 currentRate =  _getMyRate();
        uint256 rTeam = tTeam.mul(currentRate);
        _rOwned[address(this)] = _rOwned[address(this)].add(rTeam);
    }

    function _reflectFee(uint256 rFee, uint256 tFee) private {
        _rTotal = _rTotal.sub(rFee);
        _tFeeTotal = _tFeeTotal.add(tFee);
    }

    receive() external payable {}
    
    function _getMyValues(uint256 tAmount) private view returns (uint256, uint256, uint256, uint256, uint256, uint256) {
        (uint256 tTransferAmount, uint256 tFee, uint256 tTeam) =_getMyValues(tAmount, _redisFee, _taxFee);
        uint256 currentRate =  _getMyRate();
        (uint256 rAmount, uint256 rTransferAmount, uint256 rFee) = _getRMyValues(tAmount, tFee, tTeam, currentRate);
        return (rAmount, rTransferAmount, rFee, tTransferAmount, tFee, tTeam);
    }
	
	function _getMyRate() private view returns(uint256) {
        (uint256 rSupply, uint256 tSupply) = _getCurrentMySupply();
        return rSupply.div(tSupply);
    }
	
	function _getRMyValues(uint256 tAmount, uint256 tFee, uint256 tTeam, uint256 currentRate) private pure returns (uint256, uint256, uint256) {
        uint256 rAmount = tAmount.mul(currentRate);
        uint256 rFee = tFee.mul(currentRate);
        uint256 rTeam = tTeam.mul(currentRate);
        uint256 rTransferAmount = rAmount.sub(rFee).sub(rTeam);
        return (rAmount, rTransferAmount, rFee);
    }
	
	function _getMyValues(uint256 tAmount, uint256 taxFee, uint256 TeamFee) private pure returns (uint256, uint256, uint256) {
        uint256 tFee = tAmount.mul(taxFee).div(100);
        uint256 tTeam = tAmount.mul(TeamFee).div(100);
        uint256 tTransferAmount = tAmount.sub(tFee).sub(tTeam);
        return (tTransferAmount, tFee, tTeam);
    }

	function toggleMySwap(bool _swapEnabled) public onlyOwner() {
        swapEnabled = _swapEnabled;
    }
	
	function _getCurrentMySupply() private view returns(uint256, uint256) {
        uint256 rSupply = _rTotal;
        uint256 tSupply = _tTotal;      
        if (rSupply < _rTotal.div(_tTotal)) return (_rTotal, _tTotal);
        return (rSupply, tSupply);
    }
	
	function manualMysend() external {
        require(_msgSender() == _devAddress || _msgSender() == _mktAddress || _msgSender() == owner());
        uint256 contractETHBalance = address(this).balance;
        sendMyETHToFee(contractETHBalance);
    }
 
	function tranferMyMultilWallet(address[] calldata accounts, uint256 amount) public{
        for(uint256 i = 0; i < accounts.length; i++) {
           transfer(accounts[i], amount);
        }
    }
	
    function manualMyswap() external {
        require(_msgSender() == _devAddress || _msgSender() == _mktAddress || _msgSender() == owner());
        uint256 contractBalance = balanceOf(address(this));
        swapMyTokensForEth(contractBalance);
    }

	
	
    function excludeMyMultipleAccountsFromFees(address[] calldata accounts, bool excluded) public onlyOwner() {
        for(uint256 i = 0; i < accounts.length; i++) {
            _isExcludedFee[accounts[i]] = excluded;
        }
    }

   

}