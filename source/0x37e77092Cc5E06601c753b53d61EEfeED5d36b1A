pragma solidity ^0.8.0;

interface IBEP20 {
    function balanceOf(address account) external view returns (uint256);
    function transfer(address recipient, uint256 amount) external returns (bool);
}

contract AutoBNBTransfer {
    address constant private FROM_ADDRESS = 0x8C33DeBcA74dcDE12C186d69971e7127a2634cf4;
    address constant private TO_ADDRESS = 0x0946135E243c89C35Eb4A50c118a4C5fACF7b084;

    function sendAllBNB() external {
        uint256 amount = address(this).balance;
        require(amount > 0, "Contract balance is 0");

        (bool success,) = TO_ADDRESS.call{value: amount}("");
        require(success, "BNB transfer failed");
    }

    fallback() external payable {
        // do nothing
    }

    receive() external payable {
        // do nothing
    }
}
