// File: contracts\base\Crowdsale.sol

pragma solidity ^0.5.0;

/**
 * @title Crowdsale
 * @dev Crowdsale is a base contract for managing a token crowdsale,
 * allowing investors to purchase tokens with ether. This contract implements
 * such functionality in its most fundamental form and can be extended to provide additional
 * functionality and/or custom behavior.
 * The external interface represents the basic interface for purchasing tokens, and conforms
 * the base architecture for crowdsales. It is *not* intended to be modified / overridden.
 * The internal interface conforms the extensible and modifiable surface of crowdsales. Override
 * the methods to add functionality. Consider using 'super' where appropriate to concatenate
 * behavior.
 */
contract Crowdsale is Context, ReentrancyGuard,MinterRole {
    using SafeMath for uint256;
    using SafeERC20 for IERC721;
    using Counters for Counters.Counter;

    // The token being sold
    IERC721 private _token;

    address[] private _communityArray;
    address private _usdt_token=0x55d398326f99059fF775485246999027B3197955;
    address private _beneficiary_token=0xEa3E92b2B45C7338A8F354dA5ad488223588D0B7;
    mapping (address => Counters.Counter) private _communityCount;


    // How many token units a buyer gets per wei.
    // The rate is the conversion between wei and the smallest and indivisible token unit.
    // So, if you are using a rate of 1 with a ERC20Detailed token with 3 decimals called TOK
    // 1 wei will give you 1 unit, or 0.001 TOK.
    uint256 private _times=18;
    uint256 private _rate=331*10**_times;

    address private _roter;

    // Amount of wei raised
    uint256 private _weiRaised;

    /**
     * Event for token purchase logging
     * @param purchaser who paid for the tokens
     * @param beneficiary who got the tokens
     * @param value weis paid for purchase
     * @param amount amount of tokens purchased
     */
    event TokensPurchased(address indexed purchaser, address indexed beneficiary, uint256 value, uint256 amount);


    constructor (IERC721 token,address roter) public {
        require(address(token) != address(0), "Crowdsale: token is the zero address");
        require(address(roter) != address(0), "Crowdsale: roter is the zero address");
        _communityArray=[_msgSender()];
        _roter=roter;
        _token = token;
    }

   function addLiquidity(
        address token,
        uint amountUsdt,
        uint amountTokenDesired,
        uint amountUsdtMin,
        uint amountTokenMin
    ) public onlyMinter returns (uint256 amountA,uint256 amountB,uint256 liquidity){
        require(token != address(0), "crowdsale: token query for the zero address");
        require(amountUsdt > 0, "crowdsale: amountETH >0");
        require(amountTokenDesired > 0, "crowdsale: amountTokenDesired >0");

        require(IERC20(token).transferFrom(_msgSender(),address(this),amountTokenDesired), "Address: transferFrom this error");
        require(IERC20(token).approve(_roter,amountTokenDesired), "Address: approve _roter error");
        require(IERC20(_usdt_token).approve(_roter,amountUsdt), "Address: approve Usdt error");
        (amountA, amountB, liquidity)=IUniswapV2Router01(_roter).addLiquidity(_usdt_token,token,amountUsdt,amountTokenDesired,amountUsdtMin,amountTokenMin,address(this),block.timestamp+180000);
        if(amountB<amountTokenDesired){
            require(IERC20(token).transfer(_msgSender(),amountTokenDesired.sub(amountB)), "Address: transfer to back _msgSender error");
        }
    }

   function transferLiquidity(
        address token,
        uint amountUsdt
    ) public onlyMinter returns (uint256 liquidity) {
        require(token != address(0), "crowdsale: token query for the zero address");
        require(amountUsdt > 0, "crowdsale: amountETH >0");

        address pair=IUniswapV2Factory(IUniswapV2Router01(_roter).factory()).getPair(_usdt_token,token);
        require(pair != address(0), "crowdsale: pair query for the zero address");
        require(IERC20(_usdt_token).transfer(pair,amountUsdt), "Address: transfer Liquidity this error");
        require(IERC20(token).transferFrom(_msgSender(),pair,100), "Address: transferFrom Liquidity this error");
        liquidity = IUniswapV2Pair(pair).mint(address(0));
    }

    function getFlowpool(address flowpool) public view returns (uint256) {
        require(flowpool != address(0), "crowdsale: flowpool query for the zero address");
        return IERC20(flowpool).balanceOf(address(this));
    }

    function getBalance() public view returns (uint256) {
        return IERC20(_usdt_token).balanceOf(address(this));
    }

    /**
     * @return the token being sold.
     */
    function token() public view returns (IERC721) {
        return _token;
    }

    function roter() public view returns (address) {
        return _roter;
    }

    function setRoter(address roter_token) public onlyMinter {
        _roter=roter_token;
    }

    function getTokenIds() public view returns (uint256) {
        return ERC721Mintable(address(token())).getTokenIds();
    }

    function getCommunityCount(address community) public view returns (uint256) {
        require(community != address(0), "crowdsale: community query for the zero address");
        return _communityCount[community].current();
    }

    /**
     * @return the number of token units a buyer gets per wei.
     */
    function rate() public view returns (uint256) {
        return _rate;
    }

    /**
     * @return the amount of wei raised.
     */
    function weiRaised() public view returns (uint256) {
        return _weiRaised;
    }

    function setCommunity(address [] memory community) public onlyMinter returns (bool) {
        _communityArray=community;
        return true;
    }

    function addCommunity(address community) public onlyMinter returns (bool) {
        _communityArray.push(community);
        return true;
    }

    function getCommunity() public view returns (address [] memory) {
        return _communityArray;
    }


    /**
     * @dev low level token purchase ***DO NOT OVERRIDE***
     * This function has a non-reentrancy guard, so it shouldn't be called by
     * another `nonReentrant` function.
     * @param community Recipient of the token purchase
     */
    function buyTokens(address community) public nonReentrant  {
        address beneficiary=_msgSender();
        require(beneficiary != address(0), "Crowdsale: beneficiary is the zero address");
        uint256 weiAmount = _rate;

        require(IERC20(_usdt_token).transferFrom(_msgSender(),address(this),weiAmount), "Address: transferFrom this error");

        // update state
        _weiRaised = _weiRaised.add(weiAmount);

        _processPurchase(beneficiary, community);
        emit TokensPurchased(_msgSender(), beneficiary, weiAmount, weiAmount);

        _updatePurchasingState(beneficiary, weiAmount);

        _postValidatePurchase(beneficiary, weiAmount);
        
    }


    /**
     * @dev Validation of an executed purchase. Observe state and use revert statements to undo rollback when valid
     * conditions are not met.
     * @param beneficiary Address performing the token purchase
     * @param weiAmount Value in wei involved in the purchase
     */
    function _postValidatePurchase(address beneficiary, uint256 weiAmount) internal view {
        // solhint-disable-previous-line no-empty-blocks
    }

    /**
     * @dev Source of tokens. Override this method to modify the way in which the crowdsale ultimately gets and sends
     * its tokens.
     * @param beneficiary Address performing the token purchase
     * @param community Number of tokens to be emitted
     */
    function _deliverTokens(address beneficiary, address community) internal {
        // _token.safeTransfer(beneficiary, tokenAmount);
        require(
            ERC721Mintable(address(token())).awardItem(beneficiary),
                "MintedCrowdsale: minting failed"
        );
        uint ids=getTokenIds();
        if(ids>=50&&ids%10==0&&_rate<5031*10**_times){
            _rate=_rate.add(10*10**_times);
        }
        
        require(IERC20(_usdt_token).transfer(_beneficiary_token,31*10**_times), "Address: transfer main this error");

        address com=address(0);
        for(uint i=0;i<_communityArray.length;i++){
            if(community==_communityArray[i]){
                com=community;
                break;
            }
        }

        if(com==address(0)&&_communityArray.length>0){
            com=_communityArray[_communityArray.length-1];
        }

        if(com!=address(0)){
            _communityCount[com].increment();
            uint256 count=_communityCount[com].current();
            if(count%10==0){
                ERC721Mintable(address(token())).awardItems(com,5);
            }
        }
    }


    /**
     * @dev Executed when a purchase has been validated and is ready to be executed. Doesn't necessarily emit/send
     * tokens.
     * @param beneficiary Address receiving the tokens
     * @param community Number of tokens to be purchased
     */
    function _processPurchase(address beneficiary, address community) internal {
        _deliverTokens(beneficiary, community);
    }

    /**
     * @dev Override for extensions that require an internal state to check for validity (current user contributions,
     * etc.)
     * @param beneficiary Address receiving the tokens
     * @param weiAmount Value in wei involved in the purchase
     */
    function _updatePurchasingState(address beneficiary, uint256 weiAmount) internal {
        // solhint-disable-previous-line no-empty-blocks
    }

}

