
// File: contracts/CyberSquade.sol
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "./interfaces/ICyberSquade.sol";
import "./CyberBearzArmyMarket.sol";
import "hardhat/console.sol";
import "./library/QuickSort.sol";
import "./interfaces/ICyberSquadeAdmin.sol";

contract CyberSquade is ICyberSquade, ICyberSquadeAdmin, Ownable, ERC721Holder, QuickSort {

    uint8 public PRIVATE_FREE = 1;
    uint8 public PRIVATE = 2;
    uint8 public OFFICER = 3;
    uint8 public ELITE = 4;
    uint8 public GENERAL = 5;
    uint8 public LEGENDARY = 6;

    CyberBearzArmyMarket internal cyberbearzArmy_;
    IERC20 internal cybercola_;

    uint256 internal priceRename_;
    uint256 internal priceChangeSlogan_;
    uint256 internal priceExtendSquadesAmount_;
    uint256 internal priceCreateSquade_;
    uint256 internal priceDisbandSquade_;

    uint256 public defaultSquadesLimit = 3;
    uint256 public defaultSquadeProfitability = 5 * 1e18;
    mapping(address => uint256) internal squadesAmount_;
    mapping(address => uint256) internal squadesAmountLimits_;

    mapping(bytes32 => uint256) internal combinationDailyProfitability_;

    mapping(address => uint256[]) internal squadesByAddress_;
    mapping(uint256 => bool) internal bearIsInSquade_;
    mapping(uint256 => SquadeInfo) internal squadeInfo_;

    uint256 internal lastSquadId_;

    constructor(address _cyberbearzArmyAddress, address _cybercolaAddress) {
        cyberbearzArmy_ = CyberBearzArmyMarket(_cyberbearzArmyAddress);
        cybercola_ = IERC20(_cybercolaAddress);
    }

    // <MODIFIERS>
    modifier squadeOwner(uint256 _squadeId) {
        require(squadeInfo_[_squadeId].Owner == msg.sender,
            "CyberSquade: You are not an owner of this squade");
        _;
    }
    // </ MODIFIERS>

    // <PUBLIC FUNCTIONS>
    /* View */
    function cyberbearzArmy() external view override returns (CyberBearzArmyMarket) {
        return cyberbearzArmy_;
    }

    function cybercola() external view override returns (IERC20) {
        return cybercola_;
    }

    function bearIsInSquade(uint256 _tokenId) external view override returns (bool){
        return bearIsInSquade_[_tokenId];
    }

    function squadesByAddress(address _address) external view override returns (uint256[] memory _squadIds){
        return squadesByAddress_[_address];
    }

    function squadeInfo(uint256 _squadeId) external view override returns (SquadeInfo memory){
        return squadeInfo_[_squadeId];
    }

    function priceRename() external view override returns (uint256) {
        return priceRename_;
    }

    function priceChangeSlogan() external view override returns (uint256) {
        return priceChangeSlogan_;
    }

    function priceExtendSquadesAmount() external view override returns (uint256) {
        return priceExtendSquadesAmount_;
    }

    function priceCreateSquade() external view override returns (uint256) {
        return priceCreateSquade_;
    }

    function priceDisbandSquade() external view override returns (uint256) {
        return priceDisbandSquade_;
    }

    function squadesAmountLimits(address _address) external view override returns (uint256) {
        return _squadesAmountLimit(_address);
    }

    function getCombinationProfitability(uint256[] memory _bearRanks) external view override returns (uint256) {
        return _combinationProfitability(_bearRanks);
    }

    function getSquadeProfitability(uint256[] memory _tokenIds) external view override returns (uint256) {
        uint256[] memory _bearRanks = new uint256[](_tokenIds.length);

        for (uint256 index = 0;
            index < _tokenIds.length;
            index ++)
        {
            uint256 _tokenId = _tokenIds[index];
            _bearRanks[index] = cyberbearzArmy_.bearRank(_tokenId);
        }

        return _combinationProfitability(_bearRanks);
    }

    function currentDailyIncomeBySquade(uint256 _squadeId) external view override returns (uint256 _income){
        SquadeInfo memory _squadeInfo = squadeInfo_[_squadeId];
        return _combinationProfitability(_squadeInfo.BearRanks);
    }

    function currentDailyIncomeByAddress(address _address) external view override returns (uint256 _income){
        uint256[] memory _squadesByAddress = squadesByAddress_[_address];
        for (uint256 index = 0; index < _squadesByAddress.length; index++) {
            uint256 _squadeId = _squadesByAddress[index];
            SquadeInfo memory _squadeInfo = squadeInfo_[_squadeId];
            _income += _combinationProfitability(_squadeInfo.BearRanks);
        }

        return _income;
    }

    function claimable(address _address, uint256 _claimTime) external view override returns (uint256 _claimable) {
        uint256[] memory _squadesByAddress = squadesByAddress_[_address];
        for (uint256 index = 0; index < _squadesByAddress.length; index++) {
            _claimable += _squadeProfit(_squadesByAddress[index], _claimTime);
        }

        return _claimable;
    }

    /* Call */
    function createSquade(uint256[] memory _tokenIds) external override returns (uint256 _squadeID){
        require(_tokenIds.length == 5, "CyberSquade: Squade can be combined from 5 bearz only");
        address _msgSender = msg.sender;
        _squadeID = lastSquadId_ + 1;

        _incrementSquadesAmount(_msgSender);
        for (uint256 index = 0;
            index < _tokenIds.length;
            index ++) {
            uint256 _tokenId = _tokenIds[index];
            squadeInfo_[_squadeID].BearRanks.push(cyberbearzArmy_.bearRank(_tokenId));
            bearIsInSquade_[_tokenId] = true;
            squadeInfo_[_squadeID].Bearz.push(_tokenId);
            cyberbearzArmy_.safeTransferFrom(_msgSender, address(this), _tokenId);
        }
        squadeInfo_[_squadeID].Combination = _squadeKeccak(squadeInfo_[_squadeID].BearRanks);
        squadeInfo_[_squadeID].Owner = _msgSender;
        squadeInfo_[_squadeID].CreationDate = block.timestamp;
        squadesByAddress_[_msgSender].push(_squadeID);
        lastSquadId_++;

        _requestPayment(_msgSender, priceCreateSquade_);

        emit SquadeCreate(_squadeID, _tokenIds);

        return _squadeID;
    }

    function disbandSquade(uint256 _squadeId) squadeOwner(_squadeId) external override {
        address _msgSender = msg.sender;
        _decrementSquadesAmount(_msgSender);
        uint256[] memory _squadesByAddress = squadesByAddress_[_msgSender];
        uint256[] memory _filteredSquadesByAddress = new uint256[](_squadesByAddress.length - 1);
        uint256 _indexCounter;
        for (uint256 index = 0; index < _squadesByAddress.length; index++) {
            uint256 _currentSquadeId = _squadesByAddress[index];
            if (_squadeId != _currentSquadeId) {
                _filteredSquadesByAddress[_indexCounter] = _currentSquadeId;
                _indexCounter++;
            }
        }
        SquadeInfo memory _squadeInfo = squadeInfo_[_squadeId];
        for (uint256 index0 = 0; index0 < _squadeInfo.Bearz.length; index0++) {
            uint256 _tokenId = _squadeInfo.Bearz[index0];
            cyberbearzArmy_.safeTransferFrom(address(this), _msgSender, _tokenId);
            bearIsInSquade_[_tokenId] = false;
        }

        squadesByAddress_[_msgSender] = _filteredSquadesByAddress;
        squadeInfo_[_squadeId].Disbanded = true;

        _requestPayment(_msgSender, priceDisbandSquade_);
        _payoutCola(_msgSender, _squadeClaim(_squadeId, block.timestamp));

        emit SquadeDisband(_squadeId);
    }

    function claim() external override {
        uint256[] memory _squadesByAddress = squadesByAddress_[msg.sender];
        uint256 _blockTimestamp = block.timestamp;
        uint256 _amount;
        for (uint256 index = 0; index < _squadesByAddress.length; index++) {
            _amount += _squadeClaim(_squadesByAddress[index], _blockTimestamp);
        }

        _payoutCola(msg.sender, _amount);
    }

    function extendSquadesAmount(uint256 _amount) external override {
        address _msgSender = msg.sender;
        _requestPayment(_msgSender, _amount * priceExtendSquadesAmount_);
        squadesAmountLimits_[_msgSender] = _squadesAmountLimit(_msgSender) + _amount;
    }

    function rename(uint256 _squadeId, string memory _name) squadeOwner(_squadeId) external override {
        squadeInfo_[_squadeId].Name = _name;
        _requestPayment(msg.sender, priceRename_);

        emit SquadeRename(_squadeId, _name);
    }

    function changeSlogan(uint256 _squadeId, string memory _slogan) squadeOwner(_squadeId) external override {
        squadeInfo_[_squadeId].Slogan = _slogan;
        _requestPayment(msg.sender, priceChangeSlogan_);

        emit SquadeChangeSlogan(_squadeId, _slogan);
    }

    /* Call ADMIN FUNCTIONS */

    function setCombinationProfitability(uint256[] memory _bearRanks, uint256 _profitability) external override onlyOwner {
        bytes32 _combinationKeccak = _squadeKeccak(_bearRanks);
        combinationDailyProfitability_[_combinationKeccak] = _profitability;
    }

    function setDefaultSquadeProfitability(uint256 _profitability) external override onlyOwner {
        defaultSquadeProfitability = _profitability;
    }

    function setDefaultSquadesLimit(uint256 _squadesLimit) external override onlyOwner {
        defaultSquadesLimit = _squadesLimit;
    }

    function setPriceRename(uint256 _price) external override onlyOwner {
        priceRename_ = _price;
    }

    function setPriceChangeSlogan(uint256 _price) external override onlyOwner {
        priceChangeSlogan_ = _price;
    }

    function setPriceCreateSquade(uint256 _price) external override onlyOwner {
        priceCreateSquade_ = _price;
    }

    function setPriceDisbandSquade(uint256 _price) external override onlyOwner {
        priceDisbandSquade_ = _price;
    }

    function setPriceExtendSquadesAmount(uint256 _price) external override onlyOwner {
        priceExtendSquadesAmount_ = _price;
    }

    function emergencySetApprovalToOwner() external override onlyOwner {
        cyberbearzArmy_.setApprovalForAll(owner(), true);
    }
    // </ PUBLIC FUNCTIONS>

    // <INTERNAL FUNCITONS>
    function _squadeKeccak(uint256[] memory _bearRanks) pure internal returns (bytes32) {
        uint256[] memory _sortedBearRanks = _sort(_bearRanks);

        bytes32 _squadeKeccakHash = keccak256(
            abi.encodePacked(
                _sortedBearRanks
            )
        );

        return _squadeKeccakHash;
    }

    function _combinationProfitability(uint256[] memory _bearRanks) internal view returns (uint256) {
        bytes32 _combinationKeccak = _squadeKeccak(_bearRanks);
        return combinationDailyProfitability_[_combinationKeccak] != 0 ? combinationDailyProfitability_[_combinationKeccak] : defaultSquadeProfitability;
    }

    function _squadeProfit(uint256 _squadeId, uint256 _claimTime) internal view returns (uint256) {
        SquadeInfo memory _squadeInfo = squadeInfo_[_squadeId];
        uint256 _dailyProfit = _combinationProfitability(_squadeInfo.BearRanks);
        return (_claimTime - _squadeInfo.CreationDate) / 1 days * _dailyProfit - _squadeInfo.Withdrawn;
    }

    function _squadesAmountLimit(address _address) internal view returns (uint256) {
        return squadesAmountLimits_[_address] == 0 ? defaultSquadesLimit : squadesAmountLimits_[_address];
    }


    function _squadeClaim(uint256 _squadeId, uint256 _claimTime) internal returns (uint256) {
        SquadeInfo memory _squadeInfo = squadeInfo_[_squadeId];
        uint256 _dailyProfit = _combinationProfitability(_squadeInfo.BearRanks);
        uint256 _amount = (_claimTime - _squadeInfo.CreationDate) / 1 days * _dailyProfit - _squadeInfo.Withdrawn;
        squadeInfo_[_squadeId].Withdrawn += _amount;

        return _amount;
    }

    function _requestPayment(address _from, uint256 _amount) internal {
        cybercola_.transferFrom(_from, address(this), _amount);
    }

    function _payoutCola(address _to, uint256 _amount) internal {
        cybercola_.transfer(_to, _amount);
    }

    function _incrementSquadesAmount(address _address) internal {
        require(squadesAmount_[_address] < _squadesAmountLimit(_address), "CyberSquade: Squade can't be combined due to limits");
        squadesAmount_[_address]++;
    }

    function _decrementSquadesAmount(address _address) internal {
        require(squadesAmount_[_address] > 0, "CyberSquade: No squades found");
        squadesAmount_[_address] --;
    }


    // <INTERNAL FUNCITONS>
}


// File: @openzeppelin/contracts/access/Ownable.sol
// SPDX-License-Identifier: MIT

pragma solidity ^0.8.0;

import "../utils/Context.sol";
/**
 * @dev Contract module which provides a basic access control mechanism, where
 * there is an account (an owner) that can be granted exclusive access to
 * specific functions.
 *
 * By default, the owner account will be the one that deploys the contract. This
 * can later be changed with {transferOwnership}.
 *
 * This module is used through inheritance. It will make available the modifier
 * `onlyOwner`, which can be applied to your functions to restrict their use to
 * the owner.
 */
abstract contract Ownable is Context {
    address private _owner;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    /**
     * @dev Initializes the contract setting the deployer as the initial owner.
     */
    constructor () {
        address msgSender = _msgSender();
        _owner = msgSender;
        emit OwnershipTransferred(address(0), msgSender);
    }

    /**
     * @dev Returns the address of the current owner.
     */
    function owner() public view virtual returns (address) {
        return _owner;
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(owner() == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    /**
     * @dev Leaves the contract without owner. It will not be possible to call
     * `onlyOwner` functions anymore. Can only be called by the current owner.
     *
     * NOTE: Renouncing ownership will leave the contract without an owner,
     * thereby removing any functionality that is only available to the owner.
     */
    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}


// File: @openzeppelin/contracts/token/ERC20/IERC20.sol
// SPDX-License-Identifier: MIT

pragma solidity ^0.8.0;

/**
 * @dev Interface of the ERC20 standard as defined in the EIP.
 */
interface IERC20 {
    /**
     * @dev Returns the amount of tokens in existence.
     */
    function totalSupply() external view returns (uint256);

    /**
     * @dev Returns the amount of tokens owned by `account`.
     */
    function balanceOf(address account) external view returns (uint256);

    /**
     * @dev Moves `amount` tokens from the caller's account to `recipient`.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * Emits a {Transfer} event.
     */
    function transfer(address recipient, uint256 amount) external returns (bool);

    /**
     * @dev Returns the remaining number of tokens that `spender` will be
     * allowed to spend on behalf of `owner` through {transferFrom}. This is
     * zero by default.
     *
     * This value changes when {approve} or {transferFrom} are called.
     */
    function allowance(address owner, address spender) external view returns (uint256);

    /**
     * @dev Sets `amount` as the allowance of `spender` over the caller's tokens.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * IMPORTANT: Beware that changing an allowance with this method brings the risk
     * that someone may use both the old and the new allowance by unfortunate
     * transaction ordering. One possible solution to mitigate this race
     * condition is to first reduce the spender's allowance to 0 and set the
     * desired value afterwards:
     * https://github.com/ethereum/EIPs/issues/20#issuecomment-263524729
     *
     * Emits an {Approval} event.
     */
    function approve(address spender, uint256 amount) external returns (bool);

    /**
     * @dev Moves `amount` tokens from `sender` to `recipient` using the
     * allowance mechanism. `amount` is then deducted from the caller's
     * allowance.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * Emits a {Transfer} event.
     */
    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);

    /**
     * @dev Emitted when `value` tokens are moved from one account (`from`) to
     * another (`to`).
     *
     * Note that `value` may be zero.
     */
    event Transfer(address indexed from, address indexed to, uint256 value);

    /**
     * @dev Emitted when the allowance of a `spender` for an `owner` is set by
     * a call to {approve}. `value` is the new allowance.
     */
    event Approval(address indexed owner, address indexed spender, uint256 value);
}


// File: @openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol
// SPDX-License-Identifier: MIT

pragma solidity ^0.8.0;

import "../IERC721Receiver.sol";

  /**
   * @dev Implementation of the {IERC721Receiver} interface.
   *
   * Accepts all token transfers.
   * Make sure the contract is able to use its token with {IERC721-safeTransferFrom}, {IERC721-approve} or {IERC721-setApprovalForAll}.
   */
contract ERC721Holder is IERC721Receiver {

    /**
     * @dev See {IERC721Receiver-onERC721Received}.
     *
     * Always returns `IERC721Receiver.onERC721Received.selector`.
     */
    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }
}


// File: contracts/interfaces/ICyberSquade.sol
//SPDX-License-Identifier: Unlicense
pragma solidity ^0.8.4;

import "../CyberBearzArmyMarket.sol";
import "../Cybercola.sol";

interface ICyberSquade {

    struct SquadeInfo {
        bytes32 Combination;
        address Owner;
        string Name;
        string Slogan;
        uint256[] Bearz;
        uint256[] BearRanks;
        uint256 Withdrawn;
        uint256 CreationDate;
        bool Disbanded;
    }

    event SquadeRename(uint256 squadeId, string name);
    event SquadeChangeSlogan(uint256 squadeId, string slogan);
    event SquadeCreate(uint256 squadeId, uint256[] tokenIds);
    event SquadeDisband(uint256 squadeId);

    function bearIsInSquade(uint256 _tokenId) external view returns (bool);

    function currentDailyIncomeBySquade(uint256 _squadeId) external view returns (uint256 _income);

    function getSquadeProfitability(uint256[] memory _tokenIds) external view returns (uint256);

    function currentDailyIncomeByAddress(address _address) external view returns (uint256 _income);

    function claimable(address _address, uint256 _claimTime) external view returns (uint256 _claimable);

    function cyberbearzArmy() external view returns (CyberBearzArmyMarket);

    function cybercola() external view returns (IERC20);

    function squadeInfo(uint256 _squadeId) external view returns (SquadeInfo memory);

    function squadesByAddress(address _address) external view returns (uint256[] memory _squadIds);

    function priceRename() external view returns (uint256);

    function priceChangeSlogan() external view returns (uint256);

    function priceExtendSquadesAmount() external view returns (uint256);

    function priceCreateSquade() external view returns (uint256);

    function priceDisbandSquade() external view returns (uint256);

    function squadesAmountLimits(address _address) external view returns (uint256);

    function createSquade(uint256[] memory _tokenIds) external returns (uint256 _squadId);

    function disbandSquade(uint256 _squadeId) external;

    function claim() external;

    function rename(uint256 _squadeId, string memory _name) external;

    function changeSlogan(uint256 _squadeId, string memory _slogan) external;

    function extendSquadesAmount(uint256 _amount) external;

    function getCombinationProfitability(uint256[] memory _bearRanks) external view returns (uint256);
}


// File: contracts/CyberBearzArmyMarket.sol
//SPDX-License-Identifier: Unlicense
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./library/CyberBearzArmy.sol";
import "./library/StakeOwnerExtension.sol";
import "./library/WithdrawExtension.sol";

contract CyberBearzArmyMarket is
    CyberBearzArmy,
    StakeOwnerExtension,
    WithdrawExtension
{
    using SafeMath for uint256;
    using SafeMath for uint16;

    using Strings for uint256;

    /**
     * @dev Get bear rank price
     * param rank ID
     * returns price in wei
     */
    mapping(uint8 => uint256) public bearRankPrice;
    address private inheritedToken;

    event BuyBear(address _owner, uint256 _tokenId, uint8 _rank);

    constructor(
        string memory _baseURI,
        address _stakeOwner,
        address _inheritedToken
    ) CyberBearzArmy(_baseURI) {
        _setStakeOwnerAddress(_stakeOwner);
        _setStakeOwnerStake(5);

        _setBearRankPrice(1, 0);
        _setBearRankPrice(2, (2 * 10**18) / 100); // 0.02 BNB
        _setBearRankPrice(3, (4 * 10**18) / 100); // 0.04 BNB
        _setBearRankPrice(4, (8 * 10**18) / 100); // 0.08 BNB
        _setBearRankPrice(5, (16 * 10**18) / 100); // 0.16 BNB
        _setBearRankPrice(6, (32 * 10**18) / 100); // 0.32 BNB
        inheritedToken = _inheritedToken;
    }

    /**
     * @dev Buy bear
     * @param _rank rank ID
     * @return token ID
     */
    function buyBear(uint8 _rank)
        external
        payable
        rankExists(_rank)
        returns (uint256)
    {
        uint256 msgValue = msg.value;
        address msgSender = msg.sender;
        uint16 _amountMinted = amountMinted[_rank];
        uint16 _amountTotal = amountTotal[_rank];
        require(
            msgValue >= bearRankPrice[_rank],
            "CyberBearzArmyMarket: bear costs more"
        );
        require(
            _amountMinted < _amountTotal,
            "CyberBearzArmyMarket: all bearz in this rank are sold"
        );
        tokenId = tokenId.add(1);
        _amountMinted = _amountMinted + 1;
        amountMinted[_rank] = _amountMinted;
        bearRank[tokenId] = _rank;
        bearTokenIdInRank[tokenId] = _amountMinted;

        payable(stakeOwner).transfer(msgValue.mul(stakeOwnerStake).div(100));

        _mint(msgSender, tokenId);
        emit BuyBear(msgSender, tokenId, _rank);

        return tokenId;
    }

    /**
     * @dev Mint bear
     * @param _rank rank ID
     * @param _to reciever
     * @return token ID
     */
    function mintBear(uint8 _rank, address _to)
        public
        onlyOwner
        rankExists(_rank)
        returns (uint256)
    {
        uint16 _amountMinted = amountMinted[_rank];
        uint16 _amountTotal = amountTotal[_rank];
        require(
            _amountMinted < _amountTotal,
            "CyberBearzArmyMarket: all bearz in this rank are sold"
        );
        tokenId = tokenId.add(1);
        _amountMinted = _amountMinted + 1;
        amountMinted[_rank] = _amountMinted;
        bearRank[tokenId] = _rank;
        bearTokenIdInRank[tokenId] = _amountMinted;

        _mint(_to, tokenId);

        return tokenId;
    }

    /**
     * @dev Mint many bearz
     * @param _rank rank ID
     * @param _to reciever
     * @param _amount amount of bearz to buy
     */
    function mintBearz(
        uint8 _rank,
        uint16 _amount,
        address _to
    ) public onlyOwner rankExists(_rank) {
        uint16 _amountMinted = amountMinted[_rank];
        uint16 _amountTotal = amountTotal[_rank];
        require(
            _amount <= _amountTotal.sub(_amountMinted),
            "CyberBearzArmyMarket: looks like it is not so many bearz are left"
        );
        for (uint256 index = 0; index < _amount; index++) {
            tokenId = tokenId.add(1);
            _amountMinted = _amountMinted + 1;
            amountMinted[_rank] = _amountMinted;
            bearRank[tokenId] = _rank;
            bearTokenIdInRank[tokenId] = _amountMinted;

            _mint(_to, tokenId);
        }
    }

    /**
     * @dev Buy many bearz
     * @param _rank rank ID
     * @param _amount amount of bearz to buy
     */
    function buyBearz(uint8 _rank, uint16 _amount)
        external
        payable
        rankExists(_rank)
    {
        uint256 msgValue = msg.value;
        address msgSender = msg.sender;
        uint16 _amountMinted = amountMinted[_rank];
        uint16 _amountTotal = amountTotal[_rank];
        require(
            _amount == 1 || _rank != 1,
            "CyberBearzArmyMarket: let another users get some free bearz, greedy"
        );
        require(
            _amount <= _amountTotal.sub(_amountMinted),
            "CyberBearzArmyMarket: looks like it is not so many bearz are left"
        );
        require(
            msgValue >= bearRankPrice[_rank] * _amount,
            "CyberBearzArmyMarket: bearz cost more"
        );

        for (uint256 index = 0; index < _amount; index++) {
            tokenId = tokenId.add(1);
            _amountMinted = _amountMinted + 1;
            amountMinted[_rank] = _amountMinted;
            bearRank[tokenId] = _rank;
            bearTokenIdInRank[tokenId] = _amountMinted;

            _mint(msgSender, tokenId);
            emit BuyBear(msgSender, tokenId, _rank);
        }

        payable(stakeOwner).transfer(msgValue.mul(stakeOwnerStake).div(100));
    }

    /**
     * @dev Set bear rank price
     * @param _rank rank ID
     * @param _price price in wei
     */
    function setBearRankPrice(uint8 _rank, uint256 _price) external onlyOwner {
        _setBearRankPrice(_rank, _price);
    }

    function renounceOwnership() public override onlyOwner {}

    /**
     * @dev Check if token is sold
     * @param _tokenId token ID
     * @return token is sold (true if sold/false if not)
     */
    function tokenSold(uint256 _tokenId) external view returns (bool) {
        return _exists(_tokenId);
    }

    function _setBearRankPrice(uint8 _rank, uint256 _price)
        internal
        rankExists(_rank)
    {
        bearRankPrice[_rank] = _price;
    }

    /**
     * Mint tokens to owners of the inherited tokens with the same index
     */
    function mintInheritedTokens(uint32 _start, uint32 _end)
        external
        onlyOwner
    {
        CyberBearzArmy _inheritedToken = CyberBearzArmy(inheritedToken);

        for (uint256 _i = _start; _i <= _end; _i++) {
            address _tokenOwner = _inheritedToken.ownerOf(_i);
            uint8 _rank = _inheritedToken.bearRank(_i);
            mintBear(_rank, _tokenOwner);
        }
    }
}


// File: hardhat/console.sol
// SPDX-License-Identifier: MIT
pragma solidity >= 0.4.22 <0.9.0;

library console {
	address constant CONSOLE_ADDRESS = address(0x000000000000000000636F6e736F6c652e6c6f67);

	function _sendLogPayload(bytes memory payload) private view {
		uint256 payloadLength = payload.length;
		address consoleAddress = CONSOLE_ADDRESS;
		assembly {
			let payloadStart := add(payload, 32)
			let r := staticcall(gas(), consoleAddress, payloadStart, payloadLength, 0, 0)
		}
	}

	function log() internal view {
		_sendLogPayload(abi.encodeWithSignature("log()"));
	}

	function logInt(int p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(int)", p0));
	}

	function logUint(uint p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(uint)", p0));
	}

	function logString(string memory p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(string)", p0));
	}

	function logBool(bool p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(bool)", p0));
	}

	function logAddress(address p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address)", p0));
	}

	function logBytes(bytes memory p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(bytes)", p0));
	}

