pragma solidity ^0.5.0;

contract testn{

    struct one{
        uint ones;
        string ttt;
        address zzz;
    }

    address public owner;
    mapping (address => one) structone;

    constructor() public  {
        owner = msg.sender;
    }

    function testfunction(uint ones, string memory ttt, address zzz) public{
        one memory newUser;
        newUser.ones = ones;
        newUser.ttt = ttt;
        newUser.zzz = zzz;
        structone[msg.sender] = newUser;
    }
}