// File: contracts/Inokbiss.sol


pragma solidity ^0.8.4;









contract Inokbiss is ERC20, ERC20Burnable, Pausable, Ownable, ReentrancyGuard {
    using Counters for Counters.Counter;

    constructor() ERC20("Inokbiss", "IBIKS") {
        addMinter(msg.sender);
    }

    event VaultCreated(Vault vaultCreated);
    event VaultDeleted(Vault vaultDeleted);
    event BlacklistEvent(address banned, string date, string reason);
    event RemoveFromBlacklistEvent(address banned, string date, string reason);

    mapping(uint256 => Vault) public vaultForId;
    mapping(address => uint256[]) public vaultsPeerUser;

    mapping(address => bool) isBlacklisted;

    address[] public minters;
    uint256 maxSupply = 800000000000 * (10**18);

    uint256 transferFee = 0;
    uint256 ContractBalance;
    uint256 vaultWithdrawFee = 0;
    uint256 antiwhaleRestriction = 20;
    Counters.Counter private vaultId;

    /// @notice that the vault has the start time so users can verify this
    /// @notice has a coolDown that is the start time + the amount of time in secods that someone wants to lock the vault
    /// @notice that we storage the amount and the direcction of the owner of the vault

    struct Vault {
        uint256 startTime;
        uint256 coolDown;
        uint256 amount;
        address saver;
    }

    function createVault(uint256 timeInSeconds, uint256 _amount)
        public
        returns (uint256)
    {
        require(
            balanceOf(msg.sender) >= _amount,
            "Error: Your balance needs to be equal or bigger than the amount o lock"
        );
        bool transferSuccess = transferFrom(msg.sender, address(this), _amount);
        require(
            transferSuccess,
            "Error: You need to transfer the tokens to this contract"
        );

        uint256 newId = vaultId.current();

        vaultId.increment();

        vaultForId[newId] = Vault(
            block.timestamp,
            block.timestamp + timeInSeconds,
            _amount,
            msg.sender
        );
        emit VaultCreated(vaultForId[newId]);
        vaultsPeerUser[msg.sender].push(newId);
        return newId;
    }

    function withdrawVault(uint256 id) public nonReentrant {
        /// @notice that we must verify that the owner of the vault is the caller and that the lockdown time has already passed
        require(
            msg.sender == vaultForId[id].saver,
            "Error: You need to be the saver of this vault"
        );
        require(
            block.timestamp >= vaultForId[id].coolDown,
            "Error:  The cooldown has not expire yet"
        );

        uint256 amount = vaultForId[id].amount;
        /// @notice we look for the vault
        uint256 index;
        bool founded = false;
        for (uint256 i = 0; i < vaultsPeerUser[msg.sender].length; i++) {
            if (vaultsPeerUser[msg.sender][i] == id) {
                index = i;
                founded = true;
            }
        }
        require(founded, "Error: Vault not founded");

        (uint256 tax, uint256 _amount) = applyFee(amount, vaultWithdrawFee);

        emit VaultDeleted(vaultForId[id]);

        delete vaultForId[id];
        transfer(msg.sender, _amount);

        /// @notice that we are removing the vault from the user list
        vaultsPeerUser[msg.sender][uint256(index)] = vaultsPeerUser[msg.sender][
            vaultsPeerUser[msg.sender].length - 1
        ];

        vaultsPeerUser[msg.sender].pop();
    }

    /// @notice getter function for the vault of a user and to get the info of an specific vault

    function getMyVaultsId() public view returns (uint256[] memory ids) {
        ids = vaultsPeerUser[msg.sender];
        return ids;
    }

    function getMyVault(uint256 id) public view returns (Vault memory myVault) {
        myVault = vaultForId[id];
        return myVault;
    }

    modifier onlyMinters() {
        require(
            hasMinterRole(msg.sender),
            "Error: You need to have minter role to min toknes"
        );

        _;
    }

    /// @notice that the contract check if the caller is another contract
    /// @notice an also checks the balance so a wallet can never have more than the 10% of the total supply

    modifier antiWhales(address _to, uint256 _amount) {
        if (_to != address(this)) {
            require(
                (balanceOf(_to) + _amount) <
                    ((maxSupply / 100) * antiwhaleRestriction),
                "Error: One wallet can not have more than the 10% of maxSupply"
            );

            console.log(balanceOf(_to) + _amount);
            console.log(((maxSupply / 100) * antiwhaleRestriction));
        }

        _;
    }

    modifier AntiBot(address caller) {
        if (isContract(caller)) {
            require(
                caller == address(this) || hasMinterRole(caller),
                "Error this function can be executed for contract minters or this contract"
            );
        }
        _;
    }

    function isContract(address addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(addr)
        }
        return size > 0;
    }

    function setBuyAntiwhaleRestriction(uint256 newFee) public onlyOwner {
        require(newFee < 100 && newFee > 0, "Error; Max is 100");
        antiwhaleRestriction = newFee;
    }

    /// @notice Thisfunctions sets the transferFee state var
    /// @notice Documents the new must fee that can not exceeds a max amount

    function setTransferFee(uint256 newFee) public onlyOwner {
        require(newFee < 50, "Error: the fee can not exceed the 50%");
        transferFee = newFee;
    }

    function setVaultWithdrawFee(uint256 newFee) public onlyOwner {
        require(newFee < 50, "Error: the fee can not exceed the 50%");
        vaultWithdrawFee = newFee;
    }

    function hasMinterRole(address caller) public view returns (bool isMinter) {
        isMinter = false;
        for (uint256 i = 0; i < minters.length; i++) {
            if (minters[i] == caller) isMinter = true;
        }
        return isMinter;
    }

    function addMinter(address newMinter) public onlyOwner returns (bool) {
        minters.push(newMinter);
        return true;
    }

    function deleteMinter(address minter) public onlyOwner returns (bool) {
        uint256 index;
        bool founded = false;
        for (uint256 i = 0; i < vaultsPeerUser[msg.sender].length; i++) {
            if (minters[i] == minter) {
                index = i;
                founded = true;
            }
        }
        require(founded, "Error: Vault not founded");

        minters.pop();
        minters[uint256(index)] = minters[minters.length - 1];
        minters.pop();
        return (true);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function applyFee(uint256 amount, uint256 fee)
        internal
        returns (uint256 tax, uint256 _amount)
    {
        if (fee > 0) {
            tax = (amount * fee) / 100;
            _amount = amount - tax;
            ContractBalance += tax;
        } else {
            _amount = amount;
        }
        return (tax, _amount);
    }

    function mint(address to, uint256 amount) public onlyMinters {
        _mint(to, amount);
    }

    function transferInternal(address to, uint256 amount)
        internal
        returns (bool)
    {
        (uint256 tax, uint256 _amount) = applyFee(amount, transferFee);

        address owner = _msgSender();
        _transfer(owner, to, _amount);
        _transfer(owner, address(this), tax);
        return true;
    }

    function transfer(address to, uint256 amount)
        public
        override
        antiWhales(to, amount)
        AntiBot(msg.sender)
        returns (bool)
    {
        require(!isBlacklisted[msg.sender], "Error: user already blacklisted");
        require(!isBlacklisted[to], "Error: reciever is already blacklisted");

        return transferInternal(to, amount);
    }

    function transferFrom(
        address _from,
        address _to,
        uint256 _amount
    )
        public
        override
        antiWhales(_to, _amount)
        AntiBot(msg.sender)
        returns (bool)
    {
        require(!isBlacklisted[_from], "Error: the sender already blacklisted");
        require(!isBlacklisted[_to], "Error: reciever is already blacklisted");

        return transferFromInternal(_from, _to, _amount);
    }

    function transferFromInternal(
        address from,
        address to,
        uint256 amount
    ) internal returns (bool) {
        (uint256 tax, uint256 _amount) = applyFee(amount, transferFee);
        address spender = _msgSender();
        _spendAllowance(from, spender, amount);
        _transfer(from, to, _amount);
        _transfer(from, address(this), tax);
        return true;
    }

    function withdrawTokens(uint256 amount) public onlyOwner {
        require(
            ContractBalance >= amount,
            "Error: the amount exceeds the balance of the contract"
        );

        if (this.transfer(msg.sender, amount)) ContractBalance -= amount;
    }

    function withdrawBnb(uint256 amount) public onlyOwner {
        payable(msg.sender).transfer(amount);
    }

    function blackList(
        address _user,
        string memory date,
        string memory reason
    ) public onlyOwner {
        require(!isBlacklisted[_user], "user already blacklisted");
        isBlacklisted[_user] = true;
        emit BlacklistEvent(_user, date, reason);
    }

    function removeFromBlacklist(
        address _user,
        string memory date,
        string memory reason
    ) public onlyOwner {
        require(isBlacklisted[_user], "user already whitelisted");
        isBlacklisted[_user] = false;
        emit RemoveFromBlacklistEvent(_user, date, reason);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, amount);
    }
}