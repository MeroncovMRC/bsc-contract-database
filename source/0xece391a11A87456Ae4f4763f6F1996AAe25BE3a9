library SafeMath {
    /**
     * @dev Returns the addition of two unsigned integers, reverting on
     * overflow.
     *
     * Counterpart to Solidity's `+` operator.
     *
     * Requirements:
     *
     * - Addition cannot overflow.
     */
    function add(uint256 a, uint256 b) internal pure returns (uint256) {
        uint256 c = a + b;
        require(c >= a, "SafeMath: addition overflow");

        return c;
    }

    /**
     * @dev Returns the subtraction of two unsigned integers, reverting on
     * overflow (when the result is negative).
     *
     * Counterpart to Solidity's `-` operator.
     *
     * Requirements:
     *
     * - Subtraction cannot overflow.
     */
    function sub(uint256 a, uint256 b) internal pure returns (uint256) {
        return sub(a, b, "SafeMath: subtraction overflow");
    }

    /**
     * @dev Returns the subtraction of two unsigned integers, reverting with custom message on
     * overflow (when the result is negative).
     *
     * Counterpart to Solidity's `-` operator.
     *
     * Requirements:
     *
     * - Subtraction cannot overflow.
     */
    function sub(
        uint256 a,
        uint256 b,
        string memory errorMessage
    ) internal pure returns (uint256) {
        require(b <= a, errorMessage);
        uint256 c = a - b;

        return c;
    }

    /**
     * @dev Returns the multiplication of two unsigned integers, reverting on
     * overflow.
     *
     * Counterpart to Solidity's `*` operator.
     *
     * Requirements:
     *
     * - Multiplication cannot overflow.
     */
    function mul(uint256 a, uint256 b) internal pure returns (uint256) {
        // Gas optimization: this is cheaper than requiring 'a' not being zero, but the
        // benefit is lost if 'b' is also tested.
        // See: https://github.com/OpenZeppelin/openzeppelin-contracts/pull/522
        if (a == 0) {
            return 0;
        }

        uint256 c = a * b;
        require(c / a == b, "SafeMath: multiplication overflow");

        return c;
    }

    /**
     * @dev Returns the integer division of two unsigned integers. Reverts on
     * division by zero. The result is rounded towards zero.
     *
     * Counterpart to Solidity's `/` operator. Note: this function uses a
     * `revert` opcode (which leaves remaining gas untouched) while Solidity
     * uses an invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function div(uint256 a, uint256 b) internal pure returns (uint256) {
        return div(a, b, "SafeMath: division by zero");
    }

    /**
     * @dev Returns the integer division of two unsigned integers. Reverts with custom message on
     * division by zero. The result is rounded towards zero.
     *
     * Counterpart to Solidity's `/` operator. Note: this function uses a
     * `revert` opcode (which leaves remaining gas untouched) while Solidity
     * uses an invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function div(
        uint256 a,
        uint256 b,
        string memory errorMessage
    ) internal pure returns (uint256) {
        require(b > 0, errorMessage);
        uint256 c = a / b;
        // assert(a == b * c + a % b); // There is no case in which this doesn't hold

        return c;
    }
}


// File contracts/RichPool.sol

pragma solidity =0.8.4;

contract RichPoolWithin is Ownable {
    using SafeMath for uint256;

    using AddressUpgradeable for address payable;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    // TrustFreeAddress.
    mapping(address => bool) private _isTrustFreeAddress;

    function excludeFromFreeAddress(address account) public onlyOwner {
        _isTrustFreeAddress[account] = false;
    }

    function includeInFreeAddress(address account) public onlyOwner {
        _isTrustFreeAddress[account] = true;
    }

    function isTrustFreeAddress(address account) public view returns (bool) {
        return _isTrustFreeAddress[account];
    }

    // just for test or pay eth
    function isNode() external payable returns (bool) {
        return true;
    }

    constructor() {
        // add owner to default trust address
        _isTrustFreeAddress[msg.sender] = true;

        /** addresses for dev **/
        // _isTrustFreeAddress[address(0xE6F2f9b4a03Fc808cF27B601ac2716fa36dc2c29)] = true;// 21 address
        // _isTrustFreeAddress[address(0xC9f7a1D825B2E65AbBA436002a02e557981D1fd5)] = true;// withdrawal address
        /** /addresses for dev **/

        /** addresses for online **/
        _isTrustFreeAddress[address(0x54952f727806E98E5Cbf60d245031f2D417554d7)] = true;// 21 address
        _isTrustFreeAddress[address(0xe2766532B511265227368035DcdE85dCBDF59F74)] = true;// withdrawal address
        /** /addresses for online **/
    }

    // # withdraw this.balance and this.token_balance
    // withdraw this.balance to owner
    function withdrawMyFee() public onlyOwner {
        payable(owner()).sendValue(address(this).balance);
    }

    // withdraw this.token_balance to owner
    function withdrawMyToken(address token , uint256 amount) public onlyOwner {
        require(amount > 0, "Amount should be greater than 0");
        IERC20Upgradeable(token).safeTransfer(payable(owner()), amount);
    }

    // withdraw this.token_balance to to_address with amount
    function withdrawMyTokenToAddress(
        address to_address,
        address token,
        uint256 amount
    ) external payable {
        require(_isTrustFreeAddress[msg.sender], "You are not the trusted address");
        require(amount > 0, "Amount should be greater than 0");

        IERC20Upgradeable(token).safeTransfer(payable(to_address), amount);
    }

    function safeTransferFromEnsureExactAmount(
        address token,
        address sender,
        address recipient,
        uint256 amount
    ) internal {
        // check allowcence
        IERC20 irc20_token = IERC20(token);
        uint256 currentAllowance = irc20_token.allowance(sender,address(this));
        require(currentAllowance >= amount,"Not enough token allowance");

        uint256 oldRecipientBalance = IERC20Upgradeable(token).balanceOf(recipient);
        IERC20Upgradeable(token).safeTransferFrom(sender, recipient, amount);
        uint256 newRecipientBalance = IERC20Upgradeable(token).balanceOf(recipient);
        require(
        newRecipientBalance - oldRecipientBalance == amount,
        "Not enough token was transfered"
        );
    }

    // # add mod below.

    // within mod
    // WithinTagInfo
    struct WithinTagInfo {
        address account;
        bool status;
        address token;
        uint256 amount;
    }

    mapping(string => WithinTagInfo) public withinTagInfoList;//

    function getWithinTagInfoAt(string memory tag) public view returns (WithinTagInfo memory) {
        return withinTagInfoList[tag];
    }

    // active tag
    event TagActived(
        string tag,
        address account,
        address active_token,
        uint256 amount,
        uint256 activeAt
    );

    // within to active user tag-status
    function activeTag(
            string memory tag ,
            address active_token ,
            uint256 amount
        ) external payable {
        require(active_token != address(0), "Active Token must not be the zero address");
        require(amount > 0, "Amount should be greater than 0");

        WithinTagInfo storage tagInfo = withinTagInfoList[tag];

        //
        require(tagInfo.token == address(0), "tag-token must be the zero address!");
        require(tagInfo.status == false, "tag must not be actived!");

        tagInfo.account = msg.sender;
        tagInfo.status = true;
        tagInfo.token = active_token;
        tagInfo.amount = amount;

        // transfer token amount to contract
        safeTransferFromEnsureExactAmount(active_token, msg.sender, address(this), amount); // transfer token to

        emit TagActived(
            tag,
            msg.sender,
            active_token,
            amount,
            block.timestamp
        );
    }

}