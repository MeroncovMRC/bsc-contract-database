pragma solidity ^0.5.16;

interface IBEP20 {
    function release() external ;  
}

contract NextGenerationLOCKUP {
    
    address public  TOKEN_ADDRESS; // Address of the MyToken contract
    address public  OWNER; // Address of the contract owner
    
    
    constructor() public {
        
        OWNER = msg.sender;
    }

      
    function setAddress(address getAdd)  public{

        require(msg.sender == OWNER, "Only owner can call this function");
        TOKEN_ADDRESS = getAdd; 
    }
    
    function release() external {
        require(msg.sender == OWNER, "Only owner can call this function");
         IBEP20 MY_TOKEN = IBEP20(TOKEN_ADDRESS);
        MY_TOKEN.release();
    }
}