// Solidity contract example with three functions
// SPDX-License-Identifier: Unlicensed
pragma solidity ^0.8.0;

contract ExampleContract {
    uint public storedData;
    
    // Function to set the storedData value
    function setData(uint newValue) public {
        storedData = newValue;
    }
    
    // Function to get the storedData value
    function getData() public view returns (uint) {
        return storedData;
    }
    
    // Function to reset the storedData value to zero
    function resetData() public {
        storedData = 0;
    }
}