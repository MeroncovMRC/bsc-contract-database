     require(!_isExcluded[account], \"Account is already excluded\");\r\n        _accountPosition[account] = _excluded.length;\r\n        _isExcluded[account] = true;\r\n        _excluded.push(account);\r\n    }\r\n\r\n    function batchExcludeAccount(address[] memory accounts) public onlyOwner {\r\n        for (uint256 i = 0; i \u003c accounts.length; i++) {\r\n            excludeAccount(accounts[i]);\r\n        }\r\n    }\r\n\r\n    function includeAccount(address account) public onlyOwner {\r\n        require(_isExcluded[account], \"Account is already included\");\r\n        _excluded[_accountPosition[account]] = _excluded[_excluded.length - 1];\r\n        _isExcluded[account] = false;\r\n        _excluded.pop();\r\n        delete _accountPosition[account];\r\n    }\r\n   \r\n    function batchIncludeAccount(address[] memory accounts) public onlyOwner {\r\n        for (uint256 i = 0; i \u003c accounts.length; i++) {\r\n            includeAccount(accounts[i]);\r\n        }\r\n    }\r\n\r\n    function setCommunityBonusPool(address _communityBonusPool) public onlyOwner {\r\n        communityBonusPool = _communityBonusPool;\r\n    }\r\n\r\n    function setLpBonusPool(address _lpBonusPool) public onlyOwner {\r\n        // require(_lpBonusPool == address(0), \"Lp prize pool is set\");\r\n        lpBonusPool = _lpBonusPool;\r\n    }\r\n\r\n    function setReferralBonusPool(address _referralBonusPool) public onlyOwner {\r\n        referralBonusPool = _referralBonusPool;\r\n    }\r\n\r\n    function setUniswapPair(address _uniswapPair) public onlyOwner {\r\n        uniswapPair = _uniswapPair;\r\n    }\r\n\r\n}"}}