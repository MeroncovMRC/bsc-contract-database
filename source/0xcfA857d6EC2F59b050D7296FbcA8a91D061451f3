// File: src/ArcaneRuneV1.sol
pragma solidity 0.6.12;

import "@openzeppelin/contracts/math/SafeMath.sol";
import "./lib/token/BEP20/IBEP20.sol";
import "./lib/token/BEP20/SafeBEP20.sol";
import "./lib/token/BEP20/BEP20.sol";
import "./IArcaneRuneV1.sol";

abstract contract ArcaneRuneV1 is BEP20, IArcaneRuneV1 {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    uint256 public vaultFee = 0;
    uint256 public charityFee = 0;
    uint256 public devFee = 0;
    uint256 public botFee = 0;

    address public vaultAddress;
    address public charityAddress;
    address public devAddress;
    address public botAddress;

    mapping (address => bool) public isExcluded;
    address[] public excluded;

    mapping (address => bool) public isBot;
    address[] public bot;

    bool public mintable = true;

    constructor(string memory _name, string memory _symbol) public
        BEP20(_name, _symbol)
    {
    }

    function _mint(address account, uint256 amount) internal override {
        require(mintable == true, 'Minting has been forever disabled');
        require(account != address(0), 'Mint to the zero address');

        _totalSupply = _totalSupply.add(amount);
        _balances[account] = _balances[account].add(amount);

        emit Transfer(address(0), account, amount);
    }

    function mint(address _to, uint256 _amount) external override onlyOwner {
        _mint(_to, _amount);
    }

    function safe32(uint n, string memory errorMessage) internal pure returns (uint32) {
        require(n < 2**32, errorMessage);
        return uint32(n);
    }

    function getChainId() internal pure returns (uint) {
        uint256 chainId;
        assembly { chainId := chainid() }
        return chainId;
    }

    function disableMintingForever() external {
        require(_msgSender() == devAddress, "Dev only");
        mintable = false;
    }

    function setDevAddress(address _devAddress) external onlyOwner() {
        devAddress = _devAddress;
    }

    function setFeeInfo(address _vaultAddress, address _charityAddress, address _devAddress, address _botAddress, uint256 _vaultFee, uint256 _charityFee, uint256 _devFee, uint256 _botFee) external override {
        require(_msgSender() == devAddress, "Dev only");
        require (_vaultAddress != address(0) && _charityAddress != address(0) && _devAddress != address(0) && _botAddress != address(0), "Cannot use zero address for fees");
        require (_vaultFee <= 1000 && _charityFee <= 50 && _devFee <= 50 && _botFee <= 4000, "Fee constraints");

        vaultAddress = _vaultAddress;
        charityAddress = _charityAddress;
        devAddress = _devAddress;
        botAddress = _botAddress;

        vaultFee = _vaultFee;
        charityFee = _charityFee;
        devFee = _devFee;
        botFee = _botFee;
    }

    function addExcluded(address account) external override {
        require(_msgSender() == devAddress, "Dev only");
        require(!isExcluded[account], "Account is already excluded");

        isExcluded[account] = true;
        excluded.push(account);
    }

    function removeExcluded(address account) external override {
        require(_msgSender() == devAddress, "Dev only");
        require(isExcluded[account], "Account isn't excluded");

        for (uint256 i = 0; i < excluded.length; i++) {
            if (excluded[i] == account) {
                excluded[i] = excluded[excluded.length - 1];
                isExcluded[account] = false;
                excluded.pop();
                break;
            }
        }
    }

    function addBot(address account) external override {
        require(_msgSender() == devAddress, "Dev only");
        require(!isBot[account], "Account is already bot");

        isBot[account] = true;
        bot.push(account);
    }

    function removeBot(address account) external override {
        require(_msgSender() == devAddress, "Dev only");
        require(isBot[account], "Account isn't bot");

        for (uint256 i = 0; i < bot.length; i++) {
            if (bot[i] == account) {
                bot[i] = bot[bot.length - 1];
                isBot[account] = false;
                bot.pop();
                break;
            }
        }
    }

    function _transfer(address sender, address recipient, uint256 amount) internal override {
        require(sender != address(0), "Transfer from the zero address");
        require(recipient != address(0), "Transfer to the zero address");
    
        if (isBot[sender] || isBot[recipient]) {
            _transferBot(sender, recipient, amount);
        } else if (isExcluded[sender] && !isExcluded[recipient]) {
            _transferFromExcluded(sender, recipient, amount);
        } else if (!isExcluded[sender] && isExcluded[recipient]) {
            _transferToExcluded(sender, recipient, amount);
        } else if (!isExcluded[sender] && !isExcluded[recipient]) {
            _transferStandard(sender, recipient, amount);
        } else if (isExcluded[sender] && isExcluded[recipient]) {
            _transferBothExcluded(sender, recipient, amount);
        } else {
            _transferStandard(sender, recipient, amount);
        }
    }

    function _transferBot(address sender, address recipient, uint256 amount) private {
        uint256 _botFee = amount.mul(botFee).div(10000);
        uint256 transferAmount = amount.sub(_botFee);

        if (_botFee > 0) {
            emit Transfer(sender, botAddress, _botFee);
            _balances[botAddress] = _balances[botAddress].add(_botFee);
        }

        _balances[sender] = _balances[sender].sub(amount);
        _balances[recipient] = _balances[recipient].add(transferAmount);

        emit Transfer(sender, recipient, transferAmount);
    }

    function _transferStandard(address sender, address recipient, uint256 amount) private {
        uint256 _vaultFee = amount.mul(vaultFee).div(10000);
        uint256 _charityFee = amount.mul(charityFee).div(10000);
        uint256 _devFee = amount.mul(devFee).div(10000);
        uint256 transferAmount = amount.sub(_vaultFee).sub(_charityFee).sub(_devFee);

        if (_vaultFee > 0) {
            emit Transfer(sender, vaultAddress, _vaultFee);
            _balances[vaultAddress] = _balances[vaultAddress].add(_vaultFee);
        }

        if (_charityFee > 0) {
            emit Transfer(sender, charityAddress, _charityFee);
            _balances[charityAddress] = _balances[charityAddress].add(_charityFee);
        }

        if (_devFee > 0) {
            emit Transfer(sender, devAddress, _devFee);
            _balances[devAddress] = _balances[devAddress].add(_devFee);
        }

        _balances[sender] = _balances[sender].sub(amount);
        _balances[recipient] = _balances[recipient].add(transferAmount);

        emit Transfer(sender, recipient, transferAmount);
    }

    function _transferToExcluded(address sender, address recipient, uint256 amount) private {
        _balances[sender] = _balances[sender].sub(amount);
        _balances[recipient] = _balances[recipient].add(amount);

        emit Transfer(sender, recipient, amount);
    }

    function _transferFromExcluded(address sender, address recipient, uint256 amount) private {
        _balances[sender] = _balances[sender].sub(amount);
        _balances[recipient] = _balances[recipient].add(amount);

        emit Transfer(sender, recipient, amount);
    }

    function _transferBothExcluded(address sender, address recipient, uint256 amount) private {
        _balances[sender] = _balances[sender].sub(amount);
        _balances[recipient] = _balances[recipient].add(amount);

        emit Transfer(sender, recipient, amount);
    }
}

// File: src/IArcaneRuneV1.sol
pragma solidity 0.6.12;

interface IArcaneRuneV1 {
   function mint(address _to, uint256 _amount) external;
   function setFeeInfo(address _vaultAddress, address _charityAddress, address _devAddress, address _botAddress, uint256 _vaultFee, uint256 _charityFee, uint256 _devFee, uint256 _botFee) external;
   function addExcluded(address account) external;
   function removeExcluded(address account) external;
   function addBot(address account) external;
   function removeBot(address account) external;
}

// File: src/runes/Ral.sol
pragma solidity 0.6.12;

import "../ArcaneRuneV1.sol";

contract RalRune is ArcaneRuneV1('Ral', 'RAL') { /* This was a triumph! */ }

// File: src/ArcaneRaidV1.sol
pragma solidity 0.6.12;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./lib/token/BEP20/IBEP20.sol";
import "./lib/token/BEP20/BEP20.sol";
import "./lib/token/BEP20/SafeBEP20.sol";
import "./lib/ReentrancyGuard.sol";
import "./IArcaneRuneV1.sol";
import "./IArcaneMessengerV1.sol";
import "./ArcaneRaidLibraryV1.sol";

contract ArcaneRaidV1 is AccessControl, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    bytes32 public constant MESSENGER_ROLE = keccak256("MESSENGER_ROLE");

    address public currentRune;
    uint256 public totalAllocPoint = 0;
    mapping (address => uint256) public runeStartBlock;
    mapping (address => uint256) public runeEmission;
    bool public harvestAllEnabled = true;
    bool public harvestMapEnabled = false;

    address public devAddress;
    address public charityAddress;
    address public vaultAddress;
    address public voidAddress;

    // Mint percent breakdown
    uint256 public devMintPercent = 0;
    uint256 public vaultMintPercent = 0;
    uint256 public charityMintPercent = 0;

    // Deposit fee breakdown
    uint256 public devDepositPercent = 0;
    uint256 public vaultDepositPercent = 0;
    uint256 public charityDepositPercent = 0;

    ArcaneRaidLibraryV1.PoolInfo private tempPoolInfo;
    ArcaneRaidLibraryV1.PoolInfo[] public poolInfo;
    mapping (uint256 => mapping (address => ArcaneRaidLibraryV1.UserInfo)) public userInfo;
    mapping(address => bool) public poolExists;

    address[] public previousRunes;

    IArcaneMessengerV1 public messenger;

    // Cache keys prevent manipulation by external item master calls
    address cache_userAddress;
    uint256 cache_userAmount;
    uint256 cache_userRuneBalance;
    address cache_rewardRune;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);

    modifier onlyMessenger() {
        require(hasRole(MESSENGER_ROLE, _msgSender()), "Messenger only");
        _;
    }

    modifier onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "Admin only");
        _;
    }

    modifier onlyDev() {
        require(_msgSender() == devAddress, "Dev only");
        _;
    }

    modifier onlyDevOrMessenger() {
        require(_msgSender() == devAddress || hasRole(MESSENGER_ROLE, _msgSender()), "Dev or Messenger only");
        _;
    }

    modifier validatePoolExists(uint256 _pid) {
        require (_pid < poolInfo.length, "Pool does not exist");
        _;
    }

    constructor(
        address _rune,
        address _devAddress,
        address _vaultAddress,
        address _charityAddress,
        address _voidAddress,
        uint256 _runeEmission,
        uint256 runeSstartBlock
    ) public {
        currentRune = _rune;
        devAddress = _devAddress;
        vaultAddress = _vaultAddress;
        charityAddress = _charityAddress;
        voidAddress = _voidAddress;
        runeEmission[_rune] = _runeEmission;
        runeStartBlock[_rune] = runeSstartBlock;
        
        // console.log('Constructor');
        // console.log('Current Rune: ', currentRune);
        // console.log('Start Block: ', runeSstartBlock);
        // console.log('Current Block: ', block.number);

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(MESSENGER_ROLE, _msgSender());
    }

    function requireOnlyMessenger() internal onlyMessenger {

    }

    // Internal Getters
    function _getPoolLength() internal view returns (uint256) {
        return poolInfo.length;
    }

    function _getPoolAccRunePerShare(uint _pid, address _rune) internal view returns (uint256) {
        return poolInfo[_pid].accRunePerShare[_rune];
    }

    function _getPoolInfo(uint256 _pid) internal view returns (ArcaneRaidLibraryV1.PoolInfo storage) {
        return poolInfo[_pid];
    }

    function _getUserInfo(uint256 _pid, address _userAddress) internal view returns (ArcaneRaidLibraryV1.UserInfo storage) {
        return userInfo[_pid][_userAddress];
    }

    function _getUserAmount(uint256 _pid, address _userAddress) internal view returns (uint256) {
        return userInfo[_pid][_userAddress].amount;
    }

    function _getRune() internal view returns (address) {
        return currentRune;
    }

    function getPreviousRunesLength() internal view returns (uint256) {
        return previousRunes.length;
    }

    function _getPreviousRune(uint256 _index) internal view returns (address) {
        return previousRunes[_index];
    }

    function _getStartBlock(address _rune) internal view returns (uint256) {
        return runeStartBlock[_rune];
    }

    function _getCurrentUserAddress() internal view returns (address) {
        return cache_userAddress;
    }

    function getMessenger() internal view returns (IArcaneMessengerV1) {
        return messenger;
    }

    function _getTotalAllocPoint() internal view returns (uint256) {
        return totalAllocPoint;
    }

    // Internal Setters
    function _startExternalCall(ArcaneRaidLibraryV1.UserInfo storage _user, address _rewardRune) internal {
        cache_userAddress = _msgSender();
        cache_userAmount = _user.amount;
        cache_userRuneBalance = BEP20(_rewardRune).balanceOf(address(this));
        cache_rewardRune = _rewardRune;
    }

    function _endExternalCall(ArcaneRaidLibraryV1.UserInfo storage _user) internal {
        require(_user.amount >= cache_userAmount, "Messenger cannot reduce user amounts");
        // require(BEP20(cache_rewardRune).balanceOf(address(this)) >= cache_userRuneBalance, "Messenger cannot reduce user runes");
    }

    // Internal Messenger

    function _onHarvestIteration(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken, uint256 _actionId) internal returns (uint256) {
        if (address(messenger) == address(0)) return _pending;

        return messenger.onHarvestIteration(_pid, _pending, _userAddress, _rewardToken, _actionId);
    }

    function _onWithdraw(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken, uint256 _amount) internal {
        if (address(messenger) == address(0)) return;

        messenger.onWithdraw(_pid, _pending, _userAddress, _rewardToken, _amount);
    }

    function _onDeposit(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken, uint256 _amount) internal {
        if (address(messenger) == address(0)) return;

        messenger.onDeposit(_pid, _pending, _userAddress, _rewardToken, _amount);
    }

    function _onWithdrawHarvest(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken) internal returns (uint256) {
        if (address(messenger) == address(0)) return _pending;

        return messenger.onWithdrawHarvest(_pid, _pending, _userAddress, _rewardToken);
    }

    function _onDepositHarvest(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken) internal returns (uint256) {
        if (address(messenger) == address(0)) return _pending;

        return messenger.onDepositHarvest(_pid, _pending, _userAddress, _rewardToken);
    }

    function _onUpdatePool(uint256 _pid, address _rewardToken, uint256 _rewardTokenAmount, uint256 _perShare) internal {
        if (address(messenger) == address(0)) return;

        messenger.onUpdatePool(_pid, _rewardToken, _rewardTokenAmount, _perShare);
    }


    // External Messenger

    function getUserAmount(uint256 _pid) external view onlyMessenger returns (uint256) {
        return userInfo[_pid][cache_userAddress].amount;
    }

    function getPoolAccRunePerShare(uint _pid, address _rune) external view onlyMessenger returns (uint256) {
        return poolInfo[_pid].accRunePerShare[_rune];
    }

    function setUserAmount(uint256 _pid, uint256 _amount) external onlyMessenger {
        userInfo[_pid][cache_userAddress].amount = _amount;
    }

    function setUserRewardDebt(uint256 _pid, address _rune, uint256 _rewardDebt) external onlyMessenger {
        userInfo[_pid][cache_userAddress].rewardDebt[_rune] = _rewardDebt;
    }

    function updateUserRewardDebt(uint256 _pid, address _rune) external onlyMessenger {
        userInfo[_pid][cache_userAddress].rewardDebt[_rune] = userInfo[_pid][cache_userAddress].amount.mul(poolInfo[_pid].accRunePerShare[_rune]).div(1e12);
    }

    function updateUserRewardDebt(uint256 _pid, uint256 _rewardDebt) external onlyMessenger {
        userInfo[_pid][cache_userAddress].rewardDebt[currentRune] = _rewardDebt;
    }

    function updateAllUserRewardDebt(uint256 _pid) external onlyMessenger {
        userInfo[_pid][cache_userAddress].rewardDebt[currentRune] = userInfo[_pid][cache_userAddress].amount.mul(poolInfo[_pid].accRunePerShare[currentRune]).div(1e12);

        uint256 length = previousRunes.length;
        for (uint256 i = 0; i < length; ++i) {
            userInfo[_pid][cache_userAddress].rewardDebt[previousRunes[i]] = userInfo[_pid][cache_userAddress].amount.mul(poolInfo[_pid].accRunePerShare[previousRunes[i]]).div(1e12);
        }
    }

    function updateAllUserRewardDebt(uint256 _pid, uint256 _rewardDebt) external onlyMessenger {
        userInfo[_pid][cache_userAddress].rewardDebt[currentRune] = _rewardDebt;

        uint256 length = previousRunes.length;
        for (uint256 i = 0; i < length; ++i) {
            userInfo[_pid][cache_userAddress].rewardDebt[previousRunes[i]] = _rewardDebt;
        }
    }

    function harvestUserMap(ArcaneRaidLibraryV1.HarvestMap[] memory _map) external onlyMessenger {
        _harvestMap(_map, cache_userAddress);
    }

    function harvestSpecificPool(uint256 _pid, uint256 _actionId) external onlyMessenger {
        _harvestSpecificPool(_pid, cache_userAddress, _actionId);
    }

    function harvestSpecific(uint256 _pid, address _rune, uint256 _actionId) external onlyMessenger {
        _harvestSpecific(_pid, _rune, cache_userAddress, _actionId);
    }

    function harvestUserAll() external onlyMessenger {
        _harvestAll(cache_userAddress);
    }

    function updatePool(uint256 _pid, address _rune) external onlyDevOrMessenger {
        _updatePool(_pid, _rune);
    }

    function massUpdatePools() external onlyDevOrMessenger {
        _massUpdatePools(currentRune);
    }

    function safeRuneTransfer(address _address, uint256 _amount) external onlyMessenger {
        address deadAddress = 0x000000000000000000000000000000000000dEaD;

        require(_address == cache_userAddress || _address == deadAddress, "Can only send to user or burn");

        _safeRuneTransfer(cache_rewardRune, _address, _amount);
    }

    function safeBepTransfer(address _tokenAddress, address _from, address _to, uint256 _amount) external onlyMessenger {
        _safeBepTransfer(_tokenAddress, _from, _to, _amount);
    }


    // External Admin

    function setMessenger(IArcaneMessengerV1 _messenger) external onlyDev {
        messenger = _messenger;
    }

    function setHarvestFunctions(bool _harvestAllEnabled, bool _harvestMapEnabled) external onlyDev {
        harvestAllEnabled = _harvestAllEnabled;
        harvestMapEnabled = _harvestMapEnabled;
    }

    function _updateEmissionRate(address _rune, uint256 _runeEmission) internal {
        _massUpdatePools(_rune);
        runeEmission[_rune] = _runeEmission;
    }

    function updateEmissionRate(address _rune, uint256 _runeEmission) external nonReentrant onlyDev {
        _updateEmissionRate(_rune, _runeEmission);
    }

    function updateEmissionRate(uint256 _runeEmission) external nonReentrant onlyDev {
        _updateEmissionRate(currentRune, _runeEmission);
    }

    function throwRuneInVoid(address _rune) external nonReentrant onlyDev {
        require(_rune != currentRune, "Cant throw current rune in void");

        BEP20(_rune).transferOwnership(voidAddress);
    }

    function switchCurrentRune(address _rune, uint256 runeSstartBlock) external nonReentrant onlyDev {
        _massUpdatePools(currentRune);
        runeEmission[currentRune] = 0;

        previousRunes.push(currentRune);

        runeStartBlock[_rune] = block.number > runeSstartBlock ? block.number - 1 : runeSstartBlock;
        currentRune = _rune;

        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            poolInfo[pid].lastRewardBlock[_rune] = block.number > runeSstartBlock ? block.number - 1 : runeSstartBlock;
        }

        _massUpdatePools(_rune);
    }
    
    function add(uint256 _allocPoint, IBEP20 _lpToken, uint16 _depositFeeBP, bool _withUpdate) external nonReentrant onlyDev {
        require(_depositFeeBP <= 10000, "Invalid deposit fee basis points");
        require(!poolExists[address(_lpToken)], "Pool exists already");

        poolExists[address(_lpToken)] = true;
        // for (uint256 i = 0; i <= poolInfo.length; ++i) {
        //     require(poolInfo[i].lpToken != _lpToken, "Already exists");
        // }

        if (_withUpdate) {
            _massUpdatePools(currentRune);
        }

        totalAllocPoint = totalAllocPoint.add(_allocPoint);

        // console.log('Add');
        // console.log('Current Rune: ', currentRune);
        // console.log('Start Block: ', runeStartBlock[currentRune]);
        // console.log('Current Block: ', block.number);
        
        tempPoolInfo = ArcaneRaidLibraryV1.PoolInfo({
            lpToken: _lpToken,
            allocPoint: _allocPoint,
            depositFeeBP: _depositFeeBP
        });

        poolInfo.push(tempPoolInfo);

        poolInfo[poolInfo.length-1].accRunePerShare[currentRune] = 0;
        poolInfo[poolInfo.length-1].lastRewardBlock[currentRune] = block.number > runeStartBlock[currentRune] ? block.number - 1 : runeStartBlock[currentRune];

        uint256 length = previousRunes.length;
        for (uint256 i = 0; i < length; ++i) {
            poolInfo[poolInfo.length-1].accRunePerShare[previousRunes[i]] = 0;
            poolInfo[poolInfo.length-1].lastRewardBlock[previousRunes[i]] = block.number > runeStartBlock[previousRunes[i]] ? block.number - 1 : runeStartBlock[previousRunes[i]];
        }
    }

    function set(uint256 _pid, uint256 _allocPoint, uint16 _depositFeeBP, bool _withUpdate) external nonReentrant onlyDev {
        require(_depositFeeBP <= 10000, "set: invalid deposit fee basis points");

        if (_withUpdate) {
            _massUpdatePools(currentRune);
        }

        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].depositFeeBP = _depositFeeBP;
    }

    function setInfo(address _vaultAddress, address _charityAddress, address _devAddress, uint256 _vaultMintPercent, uint256 _charityMintPercent, uint256 _devMintPercent, uint256 _vaultDepositPercent, uint256 _charityDepositPercent, uint256 _devDepositPercent) external onlyDev {
        require (_vaultAddress != address(0) && _charityAddress != address(0) && _devAddress != address(0), "Cannot use zero address");
        require (_vaultMintPercent <= 39500 && _charityMintPercent <= 250 && _devMintPercent <= 450, "Mint percent constraints");
        require (_vaultDepositPercent <= 9500 && _charityDepositPercent <= 250 && _devDepositPercent <= 450, "Mint percent constraints");

        vaultAddress = _vaultAddress;
        charityAddress = _charityAddress;
        devAddress = _devAddress;

        vaultMintPercent = _vaultMintPercent;
        charityMintPercent = _charityMintPercent;
        devMintPercent = _devMintPercent;

        vaultDepositPercent = _vaultDepositPercent;
        charityDepositPercent = _charityDepositPercent;
        devDepositPercent = _devDepositPercent;
    }

    // Internal Logic

    // Safe rune transfer function, just in case if rounding error causes pool to not have enough RUNEs.
    function _safeRuneTransfer(address _rune, address _to, uint256 _amount) internal {
        IBEP20 __rune = BEP20(_rune);
        uint256 runeBal = __rune.balanceOf(address(this));

        // console.log('[ArcaneRaid] _safeRuneTransfer', _rune);
        // console.log('to', _to, 'amount', _amount);

        if (_amount > runeBal) {
            __rune.transfer(_to, runeBal);
        } else {
            __rune.transfer(_to, _amount);
        }
    }

    function _safeBepTransfer(address _tokenAddress, address _from, address _to, uint256 _amount) internal {
        require(_from != address(this), "Cant transfer from self");

        // console.log('[ArcaneRaid] _safeBepTransfer', _tokenAddress, 'from', _from);
        // console.log('to', _to, 'amount', _amount);

        IBEP20 _token = BEP20(_tokenAddress);
        uint256 _bal = _token.balanceOf(_from);

        require(_from != vaultAddress || _from == vaultAddress && ((_amount * 100) <= (_bal * 100 * 500 / 10000)), "Should not be able to transfer more than 5% from vault");

        if (_amount > _bal) {
            _token.safeTransferFrom(_from, _to, _bal);
        } else {
            _token.safeTransferFrom(_from, _to, _amount);
        }
    }

    function _updatePool(uint256 _pid, address _rune) internal {
        ArcaneRaidLibraryV1.PoolInfo storage pool = poolInfo[_pid];

        require(pool.lastRewardBlock[_rune] > 0, "Pool for this rune hasn't been setup");

        uint256 _runeEmission = runeEmission[_rune];
    
        // console.log('UpdatePool BEFORE');
        // console.log('Current Rune: ', currentRune);
        // console.log('Updating Rune: ', _rune);
        // console.log('Start Block: ', runeStartBlock[_rune]);
        // console.log('Block Number: ', block.number);
        // console.log('Per Share: ', pool.accRunePerShare[_rune]);
        // console.log('Last Reward Block: ', pool.lastRewardBlock[_rune]);
        // console.log('Rune Per Block: ', _runeEmission);

        if (block.number <= pool.lastRewardBlock[_rune]) {
            return;
        }

        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0 || pool.allocPoint == 0 || _runeEmission == 0) {
            pool.lastRewardBlock[_rune] = block.number;
            return;
        }

        uint256 multiplier = block.number.sub(pool.lastRewardBlock[_rune]);
        uint256 runeReward = multiplier.mul(_runeEmission).mul(pool.allocPoint).div(totalAllocPoint);

        if (_rune == currentRune) {
            IArcaneRuneV1(_rune).mint(devAddress, runeReward.mul(devMintPercent).div(10000));
            IArcaneRuneV1(_rune).mint(vaultAddress, runeReward.mul(vaultMintPercent).div(10000));
            IArcaneRuneV1(_rune).mint(charityAddress, runeReward.mul(charityMintPercent).div(10000));
            IArcaneRuneV1(_rune).mint(address(this), runeReward);
        }

        pool.accRunePerShare[_rune] = pool.accRunePerShare[_rune].add(runeReward.mul(1e12).div(lpSupply));
        pool.lastRewardBlock[_rune] = block.number;

        // console.log('UpdatePool AFTER');
        // console.log('Current Rune: ', currentRune);
        // console.log('Updating Rune: ', _rune);
        // console.log('Start Block: ', runeStartBlock[_rune]);
        // console.log('Block Number: ', block.number);
        // console.log('Per Share: ', pool.accRunePerShare[_rune]);
        // console.log('Last Reward Block: ', pool.lastRewardBlock[_rune]);
        // console.log('Rune Per Block: ', _runeEmission);
    }

    function _massUpdatePools(address _rune) internal {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            _updatePool(pid, _rune);
        }
    }

    function _deposit(address _rune, uint256 _pid, uint256 _amount) internal {
        require(_msgSender() != address(messenger), "Messenger can't reenter");

        ArcaneRaidLibraryV1.PoolInfo storage pool = poolInfo[_pid];
        ArcaneRaidLibraryV1.UserInfo storage user = userInfo[_pid][_msgSender()];

        require(pool.lastRewardBlock[_rune] > 0, "Pool for this rune hasn't been setup");

        _startExternalCall(user, _rune);

        _onDeposit(_pid, 0, _msgSender(), _rune, _amount);

        // console.log('Deposit');
        // console.log('Amount: ', _amount);
        // console.log('Current Rune: ', currentRune);
        // console.log('Harvest Rune: ', _rune);
        // console.log('Start Block: ', runeStartBlock[_rune]);
        // console.log('Block Number: ', block.number);
        // console.log('Per Share: ', pool.accRunePerShare[_rune]);
        // console.log('Reward Debt: ', user.rewardDebt[_rune]);

        if (_amount == 0) {
            _updatePool(_pid, _rune);

            uint256 pending = user.amount.mul(pool.accRunePerShare[_rune]).div(1e12).sub(user.rewardDebt[_rune]);

            // console.log('Pending: ', pending);

            if (pending > 0) {
                pending = _onDepositHarvest(_pid, pending, _msgSender(), _rune);
            }

            user.rewardDebt[_rune] = user.amount.mul(pool.accRunePerShare[_rune]).div(1e12);

            if (pending > 0) {
                _safeRuneTransfer(_rune, _msgSender(), pending);
            }
        }

        _endExternalCall(user);

        if (_rune == currentRune && _amount > 0) {
            _harvestSpecificPool(_pid, _msgSender(), 1);

            pool.lpToken.safeTransferFrom(address(_msgSender()), address(this), _amount);

            if (pool.depositFeeBP > 0) {
                uint256 depositFee = _amount.mul(pool.depositFeeBP).div(10000);
                pool.lpToken.safeTransfer(vaultAddress, depositFee.mul(vaultDepositPercent).div(10000));
                pool.lpToken.safeTransfer(devAddress, depositFee.mul(devDepositPercent).div(10000));
                pool.lpToken.safeTransfer(charityAddress, depositFee.mul(charityDepositPercent).div(10000));
                user.amount = user.amount.add(_amount).sub(depositFee);
            } else {
                user.amount = user.amount.add(_amount);
            }

            user.rewardDebt[currentRune] = user.amount.mul(pool.accRunePerShare[currentRune]).div(1e12);
            
            uint256 length = previousRunes.length;
            for (uint256 i = 0; i < length; ++i) {
                user.rewardDebt[previousRunes[i]] = user.amount.mul(pool.accRunePerShare[previousRunes[i]]).div(1e12);
            }
        }
 
        emit Deposit(_msgSender(), _pid, _amount);
    }

    function _withdraw(address _rune, uint256 _pid, uint256 _amount) internal {
        require(_msgSender() != address(messenger), "Messenger can't reenter");

        ArcaneRaidLibraryV1.PoolInfo storage pool = poolInfo[_pid];
        ArcaneRaidLibraryV1.UserInfo storage user = userInfo[_pid][_msgSender()];

        require(pool.lastRewardBlock[_rune] > 0, "Pool for this rune hasn't been setup");
        require(user.amount >= _amount, "Trying to withdraw too much");

        _startExternalCall(user, _rune);

        _onWithdraw(_pid, 0, _msgSender(), _rune, _amount);

        if (_amount == 0) {
            _updatePool(_pid, _rune);

            uint256 pending = user.amount.mul(pool.accRunePerShare[_rune]).div(1e12).sub(user.rewardDebt[_rune]);
        
            if (pending > 0) {
                pending = _onWithdrawHarvest(_pid, pending, _msgSender(), _rune);
            }

            user.rewardDebt[_rune] = user.amount.mul(pool.accRunePerShare[_rune]).div(1e12);

            if (pending > 0) {
                _safeRuneTransfer(_rune, _msgSender(), pending);
            }
        }

        _endExternalCall(user);

        if (_amount > 0) {
            _harvestSpecificPool(_pid, _msgSender(), 2);

            user.amount = user.amount.sub(_amount);

            pool.lpToken.safeTransfer(address(_msgSender()), _amount);

            user.rewardDebt[currentRune] = user.amount.mul(pool.accRunePerShare[currentRune]).div(1e12);
            
            uint256 length = previousRunes.length;
            for (uint256 i = 0; i < length; ++i) {
                user.rewardDebt[previousRunes[i]] = user.amount.mul(pool.accRunePerShare[previousRunes[i]]).div(1e12);
            }
        }

        emit Withdraw(_msgSender(), _pid, _amount);
    }

    // External Users

    function runePerBlock() external view returns (uint256) {
        return runeEmission[currentRune];
    }

    function startBlock() external view returns (uint256) {
        return runeStartBlock[currentRune];
    }

    function deposit(uint256 _pid, uint256 _amount) external nonReentrant validatePoolExists(_pid) {
        _deposit(currentRune, _pid, _amount);
    }

    function depositSpecific(address _rune, uint256 _pid, uint256 _amount) external nonReentrant validatePoolExists(_pid) {
        _deposit(_rune, _pid, _amount);
    }

    function withdraw(uint256 _pid, uint256 _amount) external nonReentrant validatePoolExists(_pid) {
        _withdraw(currentRune, _pid, _amount);
    }

    function withdrawSpecific(address _rune, uint256 _pid, uint256 _amount) external nonReentrant validatePoolExists(_pid) {
        _withdraw(_rune, _pid, _amount);
    }

    function _pendingRune(address _rune, uint256 _pid, address _user) internal view returns (uint256) {
        ArcaneRaidLibraryV1.PoolInfo storage pool = poolInfo[_pid];
        ArcaneRaidLibraryV1.UserInfo storage user = userInfo[_pid][_user];

        uint256 accRunePerShare = pool.accRunePerShare[_rune];
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));

        if (block.number > pool.lastRewardBlock[_rune] && lpSupply != 0) {
            uint256 _runeEmission = runeEmission[_rune];
            uint256 multiplier = block.number.sub(pool.lastRewardBlock[_rune]);
            uint256 runeReward = multiplier.mul(_runeEmission).mul(pool.allocPoint).div(totalAllocPoint);
            accRunePerShare = accRunePerShare.add(runeReward.mul(1e12).div(lpSupply));
        }

        // console.log('_pendingRune');
        // console.log('Block Number: ', block.number);
        // console.log('Current Rune: ', _rune);
        // console.log('Per Share: ', pool.accRunePerShare[_rune]);
        // console.log('Reward Debt: ', user.rewardDebt[_rune]);
        // console.log('Rune Per Block: ', _runeEmission);
        // console.log('User Amount: ', user.amount);
        // console.log('User: ', _user);
        // console.log('Last Block: ', _lastRewardBlock);

        return user.amount.mul(accRunePerShare).div(1e12).sub(user.rewardDebt[_rune]);
    }

    function pendingRune(address _rune, uint256 _pid, address _user) external view validatePoolExists(_pid) returns (uint256) {
        return _pendingRune(_rune, _pid, _user);
    }

    function pendingRune(uint256 _pid, address _user) external view validatePoolExists(_pid) returns (uint256) {
        return _pendingRune(currentRune, _pid, _user);
    }

    function _harvestMap(ArcaneRaidLibraryV1.HarvestMap[] memory _map, address _userAddress) internal {
        uint256 infoLength = _map.length;
        uint256 poolCount = poolInfo.length;

        for (uint256 i = 0; i < infoLength; ++i) {
            uint256 totalPending = 0;
            uint256 accumulatedRuneReward = 0;

            address _rune = _map[i].rune;
            uint256[] memory harvestPools = _map[i].pools;
            uint256 harvestPoolsCount = harvestPools.length;

            for (uint256 j = 0; j < harvestPoolsCount; ++j) {
                uint256 _pid = harvestPools[i];
                require(_pid < poolCount, "Pool does not exist");
                require(poolInfo[_pid].lastRewardBlock[_rune] != 0, "Reward rune does not exist");

                // console.log('HarvestSpecific');
                // console.log('Current Rune: ', currentRune);
                // console.log('Start Block: ', runeStartBlock[currentRune]);
                // console.log('Current Block: ', block.number);
                uint256 pending = 0;
                uint256 runeReward = 0;

                (runeReward, pending) = _harvestSpecific(_pid, _rune, _userAddress, 3);

                accumulatedRuneReward.add(runeReward);
                totalPending.add(pending);
            }

            if (_rune == currentRune) {
                IArcaneRuneV1(_rune).mint(devAddress, accumulatedRuneReward.mul(devMintPercent).div(10000));
                IArcaneRuneV1(_rune).mint(vaultAddress, accumulatedRuneReward.mul(vaultMintPercent).div(10000));
                IArcaneRuneV1(_rune).mint(charityAddress, accumulatedRuneReward.mul(charityMintPercent).div(10000));
                IArcaneRuneV1(_rune).mint(address(this), accumulatedRuneReward);
            }

            if (totalPending > 0) {
                // console.log('Harvest Specific: ', totalPending);

                _safeRuneTransfer(_rune, _userAddress, totalPending);
            }
        }
    }

    function harvestMap(ArcaneRaidLibraryV1.HarvestMap[] memory _map) external nonReentrant {
        if(!harvestMapEnabled) return;

        _harvestMap(_map, _msgSender());
    }

    function _harvestSpecific(uint256 _pid, address _rune, address _userAddress, uint256 _actionId) internal returns (uint256, uint256) {
        uint256 pending = 0;
        uint256 runeReward = 0;
        uint256 _runeEmission = runeEmission[_rune];
    
        ArcaneRaidLibraryV1.PoolInfo storage pool = poolInfo[_pid];
        ArcaneRaidLibraryV1.UserInfo storage user = userInfo[_pid][_userAddress];

        uint256 _accRunePerShare = pool.accRunePerShare[_rune];

        if (block.number > pool.lastRewardBlock[_rune]) {
            uint256 lpSupply = pool.lpToken.balanceOf(address(this));
            if (lpSupply == 0 || pool.allocPoint == 0 || _runeEmission == 0) {
                pool.lastRewardBlock[_rune] = block.number;
            } else {
                uint256 multiplier = block.number.sub(pool.lastRewardBlock[_rune]);
                runeReward = multiplier.mul(_runeEmission).mul(pool.allocPoint).div(totalAllocPoint);

                pool.accRunePerShare[_rune] = _accRunePerShare.add(runeReward.mul(1e12).div(lpSupply));
                pool.lastRewardBlock[_rune] = block.number;
            }
        }

        _accRunePerShare = pool.accRunePerShare[_rune];
        pending = user.amount.mul(_accRunePerShare).div(1e12).sub(user.rewardDebt[_rune]);
        
        // console.log('Harvest pre-iteration pending:', pending);

        if (pending > 0) {
            _startExternalCall(user, _rune);

            // console.log('Harvest iteration pending:', pending);
            pending = _onHarvestIteration(_pid, pending, _userAddress, _rune, _actionId);

            _endExternalCall(user);
        }

        user.rewardDebt[_rune] = user.amount.mul(_accRunePerShare).div(1e12);

        return (runeReward, pending);
    }

    function _harvestSpecificPool(uint256 _pid, address _userAddress, uint256 _actionId) internal {
        uint256 runeReward;
        uint256 pending;

        (runeReward, pending) = _harvestSpecific(_pid, currentRune, _userAddress, _actionId);

        IArcaneRuneV1(currentRune).mint(devAddress, runeReward.mul(devMintPercent).div(10000));
        IArcaneRuneV1(currentRune).mint(vaultAddress, runeReward.mul(vaultMintPercent).div(10000));
        IArcaneRuneV1(currentRune).mint(charityAddress, runeReward.mul(charityMintPercent).div(10000));
        IArcaneRuneV1(currentRune).mint(address(this), runeReward);

        if (pending > 0) {
            _safeRuneTransfer(currentRune, _userAddress, pending);
        }

        uint256 length = previousRunes.length;
        for (uint256 i = 0; i < length; ++i) {
            address _rune = previousRunes[i];

            (runeReward, pending) = _harvestSpecific(_pid, _rune, _userAddress, _actionId);

            if (pending > 0) {
                _safeRuneTransfer(_rune, _userAddress, pending);
            }
        }
    }

    function _harvestAll(address _userAddress) internal {
        uint256 length = poolInfo.length;
        for (uint256 _pid = 0; _pid < length; ++_pid) {
            _harvestSpecificPool(_pid, _userAddress, 3);
        }
    }

    function harvestAll() external nonReentrant {
        if(!harvestAllEnabled) return;

        _harvestAll(_msgSender());
    }

    function emergencyWithdraw(uint256 _pid) external nonReentrant validatePoolExists(_pid) {
        require(_msgSender() != address(messenger), "Messenger can't enter");

        ArcaneRaidLibraryV1.PoolInfo storage pool = poolInfo[_pid];
        ArcaneRaidLibraryV1.UserInfo storage user = userInfo[_pid][_msgSender()];

        userInfo[_pid][_msgSender()].rewardDebt[currentRune] = 0;
        
        uint256 length = previousRunes.length;
        for (uint256 i = 0; i < length; ++i) {
            userInfo[_pid][_msgSender()].rewardDebt[previousRunes[i]] = 0;
        }

        uint256 amount = user.amount;

        user.amount = 0;

        pool.lpToken.safeTransfer(address(_msgSender()), amount);

        emit EmergencyWithdraw(_msgSender(), _pid, amount);
    }
}


// File: src/lib/ReentrancyGuard.sol
pragma solidity >= 0.6.12;

/**
 * @dev Contract module that helps prevent reentrant calls to a function.
 *
 * Inheriting from `ReentrancyGuard` will make the {nonReentrant} modifier
 * available, which can be applied to functions to make sure there are no nested
 * (reentrant) calls to them.
 *
 * Note that because there is a single `nonReentrant` guard, functions marked as
 * `nonReentrant` may not call one another. This can be worked around by making
 * those functions `private`, and then adding `external` `nonReentrant` entry
 * points to them.
 *
 * TIP: If you would like to learn more about reentrancy and alternative ways
 * to protect against it, check out our blog post
 * https://blog.openzeppelin.com/reentrancy-after-istanbul/[Reentrancy After Istanbul].
 */
abstract contract ReentrancyGuard {
    // Booleans are more expensive than uint256 or any type that takes up a full
    // word because each write operation emits an extra SLOAD to first read the
    // slot's contents, replace the bits taken up by the boolean, and then write
    // back. This is the compiler's defense against contract upgrades and
    // pointer aliasing, and it cannot be disabled.

    // The values being non-zero value makes deployment a bit more expensive,
    // but in exchange the refund on every call to nonReentrant will be lower in
    // amount. Since refunds are capped to a percentage of the total
    // transaction's gas, it is best to keep them low in cases like this one, to
    // increase the likelihood of the full refund coming into effect.
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;

    uint256 private _status;

    constructor () public {
        _status = _NOT_ENTERED;
    }

    /**
     * @dev Prevents a contract from calling itself, directly or indirectly.
     * Calling a `nonReentrant` function from another `nonReentrant`
     * function is not supported. It is possible to prevent this from happening
     * by making the `nonReentrant` function external, and make it call a
     * `private` function that does the actual work.
     */
    modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        _status = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }
}

// File: src/IArcaneMessengerV1.sol
pragma solidity ^0.6.0;

interface IArcaneMessengerV1 {
    function onHarvestIteration(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken, uint256 _actionId) external returns (uint256);
    function onWithdraw(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken, uint256 _amount) external;
    function onDeposit(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken, uint256 _amount) external;
    function onWithdrawHarvest(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken) external returns (uint256);
    function onDepositHarvest(uint256 _pid, uint256 _pending, address _userAddress, address _rewardToken) external returns (uint256);
    function onUpdatePool(uint256 _pid, address _runeReward, uint256 _runeRewardAmount, uint256 _perShare) external;
}

// File: src/ArcaneRaidLibraryV1.sol
pragma solidity 0.6.12;

import "@openzeppelin/contracts/math/SafeMath.sol";
import "./lib/token/BEP20/IBEP20.sol";
import "./lib/token/BEP20/SafeBEP20.sol";

library ArcaneRaidLibraryV1 {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    struct UserInfo {
        uint256 amount;                              // How many LP tokens the user has provided.
        mapping (address => uint256) rewardDebt;     // Reward debt. See explanation below.
        //
        // We do some fancy math here. Basically, any point in time, the amount of RUNEs
        // entitled to a user but is pending to be distributed is:
        //
        //   pending reward = (user.amount * pool.accRunePerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws LP tokens to a pool. Here's what happens:
        //   1. The pool's `accRunePerShare` (and `lastRewardBlock`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }

    struct PoolInfo {
        IBEP20 lpToken;                                 // Address of LP token contract.
        uint256 allocPoint;                             // How many allocation points assigned to this pool. RUNEs to distribute per block.
        mapping (address => uint256) lastRewardBlock;   // Last block number that RUNEs distribution occurs.
        mapping (address => uint256) accRunePerShare;   // Accumulated RUNEs per share, times 1e12. See below.
        uint16 depositFeeBP;                            // Deposit fee in basis points
    }

    struct HarvestMap {
        address rune;
        uint256[] pools;
    }
}

// File: src/IArcaneRaidV1.sol
pragma solidity ^0.6.0;

import "./IArcaneMessengerV1.sol";

interface IArcaneRaidV1 {
