ipient == liquidityPairAddress) {\r\n                feeRate = sellTaxes.liquidity + sellTaxes.treasury;\r\n                currentTaxes = sellTaxes;\r\n            }\r\n            // This is a PancakeSwap Buy or regular transaction\r\n            else {\r\n                feeRate = taxes.liquidity + taxes.treasury;\r\n                currentTaxes = taxes;\r\n            }\r\n\r\n            // The operation is not performed on PancakeSwap Buy transactions\r\n            if (providingLiquidity \u0026\u0026 sender != liquidityPairAddress) {\r\n                liquify(feeRate, currentTaxes);\r\n            }\r\n\r\n            fee = amount * feeRate / 100;\r\n\r\n            // Add RULE fee to smart contract balance\r\n            super._transfer(sender, address(this), fee);\r\n        }\r\n\r\n        // Send RULE to recipient (taxed if fee\u003e0)\r\n        super._transfer(sender, recipient, amount - fee);\r\n    }\r\n\r\n    // #endregion\r\n\r\n    // #region Private Functions\r\n\r\n    function liquify(uint256 swapFee, Taxes memory swapTaxes) private lockLiquify {\r\n        uint256 contractBalance = balanceOf(address(this));\r\n        if (contractBalance \u003e= liquidityThreshold) {\r\n            if (liquidityThreshold \u003e 1) {\r\n                contractBalance = liquidityThreshold;\r\n            }    \r\n            \r\n            uint256 denominator = swapFee * 2;\r\n\r\n            uint256 ruleForLiquidity = contractBalance * swapTaxes.liquidity / denominator;\r\n            uint256 ruleToSwap = contractBalance - ruleForLiquidity;\r\n\r\n            uint256 bnbInitialBalance = address(this).balance;\r\n\r\n            swapRULEForBNB(ruleToSwap);\r\n\r\n            uint256 bnbDeltaBalance = address(this).balance - bnbInitialBalance;\r\n            uint256 bnbUnitBalance = bnbDeltaBalance / (denominator - swapTaxes.liquidity);       \r\n            uint256 bnbForLiquidity = bnbUnitBalance * swapTaxes.liquidity;\r\n\r\n            if (bnbForLiquidity \u003e 0) {\r\n                addLiquidity(ruleForLiquidity, bnbForLiquidity);\r\n            }\r\n\r\n            uint256 bnbForTreasury = bnbUnitBalance * 2 * swapTaxes.treasury;\r\n\r\n            if (bnbForTreasury \u003e 0) {\r\n                payable(ruleTreasuryWallet).sendValue(bnbForTreasury);\r\n            }           \r\n        }\r\n    }\r\n\r\n    function swapRULEForBNB(uint256 tokenAmount) private {\r\n        address[] memory path = new address[](2);\r\n        path[0] = address(this);\r\n        path[1] = router.WETH();\r\n\r\n        _approve(address(this), address(router), tokenAmount);\r\n\r\n        router.swapExactTokensForETHSupportingFeeOnTransferTokens(\r\n            tokenAmount,\r\n            0,\r\n            path,\r\n            address(this),\r\n            block.timestamp\r\n        );\r\n    }\r\n\r\n    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {\r\n        _approve(address(this), address(router), tokenAmount);\r\n        router.addLiquidityETH{value: ethAmount}(address(this), tokenAmount, 0, 0, BLACK_HOLE_ADDRESS, block.timestamp);\r\n    }\r\n\r\n    // #endregion\r\n}"}}