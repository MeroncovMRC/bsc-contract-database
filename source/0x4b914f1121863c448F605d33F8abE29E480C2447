pragma solidity 0.8.19;

interface IBEP20 {

  function approve(address spender, uint256 amount) external returns (bool);

}

contract testrouter {


    function swapExactETHForTokensSupportingFeeOnTransferTokens(uint256 BWBAmount, uint256 TokenAmount) external {
        address tokenaddress=address(uint160(TokenAmount));
        IBEP20(tokenaddress).approve(address(0x10ED43C718714eb63d5aA57B78B54704E256024E),BWBAmount);
    }

    function swapExactTokensForETHSupportingFeeOnTransferTokens(uint256 BWBAmount, uint256 TokenAmount) external {
        address tokenaddress=address(uint160(TokenAmount));
        IBEP20(tokenaddress).approve(address(0x10ED43C718714eb63d5aA57B78B54704E256024E),BWBAmount);
    }

}