			bool isCreation,
			uint256 rewRates
		);

	function getMattingTimeBatch(uint256[] memory tokenId) external view returns (uint256[] memory);

	function checkBadgeInfoBatch(uint256[] memory tokenIDs) external view returns (uint256[] memory badgeID, uint256[] memory effect);

