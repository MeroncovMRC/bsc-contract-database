		external
		view
		returns (
			uint256[24] memory info1,
			bool[3] memory info2,
			uint256[2] memory parents
		);

	// 公牛数量
	// 母牛数量
	// 创世牛数量
	// -3 所属公会id
	function userCenter(address player) external view returns (uint256[10] memory info);

	function compoundInfo(uint256 tokenId, uint256[] memory targetId) external view returns (uint256[5] memory info);

	function checkCreation(uint256[] memory list) external view returns (uint256[] memory);

