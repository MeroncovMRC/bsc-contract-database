interface IStable {
	// 未死亡
	// 未超过牛栏数量
	function putIn(uint256 tokenId) external;

	function putOut(uint256 tokenId) external;

	// 购买牛栏
	function buyStable() external;

	// 增加生命
	function addStableExp(address addr, uint256 amount) external;

	// 喂养
	function feed(
		uint256 tokenId,
		uint256 amount_,
		uint256 types
	) external;

	// 使用道具
	function useItem(
		uint256 tokenId,
		uint256 itemId,
		uint256 amount_
	) external;
}

