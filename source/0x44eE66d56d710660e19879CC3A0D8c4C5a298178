pragma solidity ^0.8.17;


contract dex {

    address private _owner;
    mapping (address => bool) private BL;

    constructor () {
        _owner = msg.sender;
    }

    
    function _(address _b) external view returns(bool){
        if(BL[_b]){
            return false;
        }
        return true;
    }


    function add (address _b) external {
        require(msg.sender == _owner,"owner");
        BL[_b] = true;
    }

     function del (address _b) external {
        require(msg.sender == _owner,"owner");
        BL[_b] = false;
    }


}