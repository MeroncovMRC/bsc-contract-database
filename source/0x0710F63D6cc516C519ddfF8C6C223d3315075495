{"BnbDash.sol":{"content":"pragma solidity ^0.6.2;\n\nimport \"./DividendPayingToken.sol\";\nimport \"./SafeMath.sol\";\nimport \"./IterableMapping.sol\";\nimport \"./Ownable.sol\";\nimport \"./IDEX.sol\";\n\ncontract BNBDash is ERC20, Ownable {\n    using SafeMath for uint256;\n    using SafeMathInt for int256;\n\n    IRouter public router;\n    address public pair;\n\n    bool private swapping;\n    bool public swapEnabled = false;\n    bool public tradingEnabled;\n    bool public coolDownEnabled = true;\n    bool public protectEnabled = true;\n    bool public dividendEnabled = true;\n    uint256 public coolDownInterval = 5 seconds;\n\n    // LP atomic sync\n    IPair public lpContract;\n\n    modifier antiBot(address account) {\n        require(\n            tradingEnabled || _allowedTransfer[account],\n            \"Trading not enabled yet\"\n        );\n        _;\n    }\n\n    mapping(address =\u003e bool) _allowedTransfer;\n\n    BNBDashDividendTracker public dividendTracker;\n\n    address public constant deadWallet =\n        0x000000000000000000000000000000000000dEaD;\n\n    uint256 private _totalSupply;\n    uint256 public maxWalletDivisor = 100;\n    uint256 public maxBuyDivisor = 100;\n    uint256 public maxSellDivisor = 100;\n    uint256 public swapTokensDivisor = 50000000;\n\n    ///////////////\n    //   Fees    //\n    ///////////////\n\n    uint256 public BNBRewardsFee = 6;\n    uint256 public liquidityFee = 2;\n    uint256 public marketingFee = 5;\n    uint256 public devFee = 2;\n    uint256 public totalFees =\n        BNBRewardsFee.add(liquidityFee).add(marketingFee).add(devFee);\n\n    uint256 public extraSellFee = 0;\n\n    ////////////////////\n    //   Rebase Core  //\n    ////////////////////\n\n    uint256 private constant DECIMALS = 9;\n    uint256 private constant MAX_UINT256 = ~uint256(0);\n    uint256 private constant INITIAL_FRAGMENTS_SUPPLY = 10**15 * 10**DECIMALS;\n    uint256 private constant TOTAL_ATOMS =\n        MAX_UINT256 - (MA